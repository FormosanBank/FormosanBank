    <S id="229932">
        <FORM kindOf="original">maca a maca,</FORM>
        <FORM kindOf="standard">maca a maca,</FORM>
        <TRANSL xml:lang="zh">眼睛呀眼睛,</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229932.mp3" url="https://web.klokah.tw/text/sound//17641/229932.mp3"/>
    </S>
    <S id="229936">
        <FORM kindOf="original">mena'u da eman?</FORM>
        <FORM kindOf="standard">mena'o da eman?</FORM>
        <TRANSL xml:lang="zh">看見了什麼?</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229936.mp3" url="https://web.klokah.tw/text/sound//17641/229936.mp3"/>
    </S>
    <S id="229937">
        <FORM kindOf="original">mena'u da valray,</FORM>
        <FORM kindOf="standard">mena'o da valray,</FORM>
        <TRANSL xml:lang="zh">看見了書,</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229937.mp3" url="https://web.klokah.tw/text/sound//17641/229937.mp3"/>
    </S>
    <S id="229940">
        <FORM kindOf="original">acevung mi da cali,</FORM>
        <FORM kindOf="standard">acevong mi da cali,</FORM>
        <TRANSL xml:lang="zh">我們在路上撿到一條繩子,</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229940.mp3" url="https://web.klokah.tw/text/sound//17642/229940.mp3"/>
    </S>
    <S id="229946">
        <FORM kindOf="original">ziyang kēng poy. ziyang kēng poy.</FORM>
        <FORM kindOf="standard">ziyang kēng poy. ziyang kēng poy.</FORM>
        <TRANSL xml:lang="zh">(口白)黑白猜。 (口白)黑白猜。</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229946.mp3" url="https://web.klokah.tw/text/sound//17642/229946.mp3"/>
    </S>
    <S id="229947">
        <FORM kindOf="original">mukuwa mi i Puyuma,</FORM>
        <FORM kindOf="standard">mokowa mi i Poyoma,</FORM>
        <TRANSL xml:lang="zh">我們去南王,</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229947.mp3" url="https://web.klokah.tw/text/sound//17643/229947.mp3"/>
    </S>
    <S id="229949">
        <FORM kindOf="original">tu 'apiyaraanay na walru,</FORM>
        <FORM kindOf="standard">to 'apiyaraanay na walro,</FORM>
        <TRANSL xml:lang="zh">他們撒落糖果,</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229949.mp3" url="https://web.klokah.tw/text/sound//17643/229949.mp3"/>
    </S>
    <S id="229951">
        <FORM kindOf="original">kurelrang ku ma'ara'araw da walru,</FORM>
        <FORM kindOf="standard">korelrang ko ma'ara'araw da walro,</FORM>
        <TRANSL xml:lang="zh">我也參與了搶糖果活動,</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229951.mp3" url="https://web.klokah.tw/text/sound//17643/229951.mp3"/>
    </S>
    <S id="229953">
        <FORM kindOf="original">kasahasahar i kungkuwan,</FORM>
        <FORM kindOf="standard">kasahasahar i kongkowan,</FORM>
        <TRANSL xml:lang="zh">學校真好玩,</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229953.mp3" url="https://web.klokah.tw/text/sound//17644/229953.mp3"/>
    </S>
    <S id="229957">
        <FORM kindOf="original">semangal mi malihilihi.</FORM>
        <FORM kindOf="standard">semangal mi malihilihi.</FORM>
        <TRANSL xml:lang="zh">我們快樂的遊戲。</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229957.mp3" url="https://web.klokah.tw/text/sound//17644/229957.mp3"/>
    </S>
    <S id="229938">
        <FORM kindOf="original">mena'u diya eman?</FORM>
        <FORM kindOf="standard">mena'o diya eman?</FORM>
        <TRANSL xml:lang="zh">還看見了什麼?</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229938.mp3" url="https://web.klokah.tw/text/sound//17641/229938.mp3"/>
    </S>
    <S id="229939">
        <FORM kindOf="original">mena'u da'apuc.</FORM>
        <FORM kindOf="standard">mena'o da'apoc.</FORM>
        <TRANSL xml:lang="zh">還看見了花。</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229939.mp3" url="https://web.klokah.tw/text/sound//17641/229939.mp3"/>
    </S>
    <S id="229941">
        <FORM kindOf="original">uri awa munkunkun mi kana cali.</FORM>
        <FORM kindOf="standard">ori awa monkonkon mi kana cali.</FORM>
        <TRANSL xml:lang="zh">我們拿它來跳繩。</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229941.mp3" url="https://web.klokah.tw/text/sound//17642/229941.mp3"/>
    </S>
    <S id="229934">
        <FORM kindOf="original">ma'idang ku maca.</FORM>
        <FORM kindOf="standard">ma'idang ko maca.</FORM>
        <TRANSL xml:lang="zh">我的眼睛很大;</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229934.mp3" url="https://web.klokah.tw/text/sound//17641/229934.mp3"/>
    </S>
    <S id="229944">
        <FORM kindOf="original">mumu a taramaway mi.</FORM>
        <FORM kindOf="standard">momo a taramaway mi.</FORM>
        <TRANSL xml:lang="zh">祖母啊!幫我們祈福。</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229944.mp3" url="https://web.klokah.tw/text/sound//17642/229944.mp3"/>
    </S>
    <S id="229935">
        <FORM kindOf="original">makiteng nu maca.</FORM>
        <FORM kindOf="standard">makiteng no maca.</FORM>
        <TRANSL xml:lang="zh">你的眼睛很小。</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229935.mp3" url="https://web.klokah.tw/text/sound//17641/229935.mp3"/>
    </S>
    <S id="229933">
        <FORM kindOf="original">duwaya ku maca,</FORM>
        <FORM kindOf="standard">dowaya ko maca,</FORM>
        <TRANSL xml:lang="zh">我有兩隻眼睛,</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229933.mp3" url="https://web.klokah.tw/text/sound//17641/229933.mp3"/>
    </S>
    <S id="229943">
        <FORM kindOf="original">piya iyan? piya 'ami.</FORM>
        <FORM kindOf="standard">piya iyan? piya 'ami.</FORM>
        <TRANSL xml:lang="zh">向哪裡?向北邊。</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229943.mp3" url="https://web.klokah.tw/text/sound//17642/229943.mp3"/>
    </S>
    <S id="229984">
        <FORM kindOf="original">adi repidan mu, saluvukan lra,</FORM>
        <FORM kindOf="standard">adi repidan mo, salovokan lra,</FORM>
        <TRANSL xml:lang="zh">一會兒又是一袋袋,</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229984.mp3" url="https://web.klokah.tw/text/sound//17649/229984.mp3"/>
    </S>
    <S id="229942">
        <FORM kindOf="original">munkunkun mi di masasenay mi,</FORM>
        <FORM kindOf="standard">monkonkon mi di masasenay mi,</FORM>
        <TRANSL xml:lang="zh">我們邊跳邊唱:</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229942.mp3" url="https://web.klokah.tw/text/sound//17642/229942.mp3"/>
    </S>
    <S id="229972">
        <FORM kindOf="original">intaw mu Kacacipulr,</FORM>
        <FORM kindOf="standard">intaw mo Kacacipolr,</FORM>
        <TRANSL xml:lang="zh">他是知本的,</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229972.mp3" url="https://web.klokah.tw/text/sound//17647/229972.mp3"/>
    </S>
    <S id="229948">
        <FORM kindOf="original">vasivas lra kema i malri,</FORM>
        <FORM kindOf="standard">vasivas lra kema i malri,</FORM>
        <TRANSL xml:lang="zh">爸爸說是少年年祭了,</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229948.mp3" url="https://web.klokah.tw/text/sound//17643/229948.mp3"/>
    </S>
    <S id="229950">
        <FORM kindOf="original">kemay icas kana takuvang,</FORM>
        <FORM kindOf="standard">kemay icas kana takovang,</FORM>
        <TRANSL xml:lang="zh">是從少年會所的上面向下撒,</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229950.mp3" url="https://web.klokah.tw/text/sound//17643/229950.mp3"/>
    </S>
    <S id="229952">
        <FORM kindOf="original">salaw kasahasahar.</FORM>
        <FORM kindOf="standard">salaw kasahasahar.</FORM>
        <TRANSL xml:lang="zh">非常有趣。</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229952.mp3" url="https://web.klokah.tw/text/sound//17643/229952.mp3"/>
    </S>
    <S id="229955">
        <FORM kindOf="original">temakesi di zemenga,</FORM>
        <FORM kindOf="standard">temakesi di zemenga,</FORM>
        <TRANSL xml:lang="zh">讀書和畫畫,</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229955.mp3" url="https://web.klokah.tw/text/sound//17644/229955.mp3"/>
    </S>
    <S id="229958">
        <FORM kindOf="original">sahar da velrvelr na ungay,</FORM>
        <FORM kindOf="standard">sahar da velrvelr na ongay,</FORM>
        <TRANSL xml:lang="zh">猴子愛吃香蕉,</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229958.mp3" url="https://web.klokah.tw/text/sound//17645/229958.mp3"/>
    </S>
    <S id="229960">
        <FORM kindOf="original">tu piyayaw mekan na velrvelr na ungay,</FORM>
        <FORM kindOf="standard">to piyayaw mekan na velrvelr na ongay,</FORM>
        <TRANSL xml:lang="zh">猴子把香蕉吃光光,</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229960.mp3" url="https://web.klokah.tw/text/sound//17645/229960.mp3"/>
    </S>
    <S id="229968">
        <FORM kindOf="original">vulay a vulay ta tikelruwan,</FORM>
        <FORM kindOf="standard">volay a volay ta tikelrowan,</FORM>
        <TRANSL xml:lang="zh">漂亮的鞦韆,</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229968.mp3" url="https://web.klokah.tw/text/sound//17646/229968.mp3"/>
    </S>
    <S id="229969">
        <FORM kindOf="original">'amiyan a 'amiyan kasahasahar.</FORM>
        <FORM kindOf="standard">'amiyan a 'amiyan kasahasahar.</FORM>
        <TRANSL xml:lang="zh">年祭呀年祭!真熱鬧!</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229969.mp3" url="https://web.klokah.tw/text/sound//17646/229969.mp3"/>
    </S>
    <S id="229962">
        <FORM kindOf="original">minacay tu velrvelr piniya na ungay,</FORM>
        <FORM kindOf="standard">minacay to velrvelr piniya na ongay,</FORM>
        <TRANSL xml:lang="zh">猴子的香蕉死光光,</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229962.mp3" url="https://web.klokah.tw/text/sound//17645/229962.mp3"/>
    </S>
    <S id="229959">
        <FORM kindOf="original">adi sahar da velrevelr na turu,</FORM>
        <FORM kindOf="standard">adi sahar da velrevelr na toro,</FORM>
        <TRANSL xml:lang="zh">烏龜不愛吃香蕉,</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229959.mp3" url="https://web.klokah.tw/text/sound//17645/229959.mp3"/>
    </S>
    <S id="229967">
        <FORM kindOf="original">acas a acas ta tikelruwan,</FORM>
        <FORM kindOf="standard">acas a acas ta tikelrowan,</FORM>
        <TRANSL xml:lang="zh">高高的鞦韆,</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229967.mp3" url="https://web.klokah.tw/text/sound//17646/229967.mp3"/>
    </S>
    <S id="229966">
        <FORM kindOf="original">ma'idang a ma'idang ta tikelruwan,</FORM>
        <FORM kindOf="standard">ma'idang a ma'idang ta tikelrowan,</FORM>
        <TRANSL xml:lang="zh">大大的鞦韆,</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229966.mp3" url="https://web.klokah.tw/text/sound//17646/229966.mp3"/>
    </S>
    <S id="229965">
        <FORM kindOf="original">palakuwan a palakuwan ta tikelruwan,</FORM>
        <FORM kindOf="standard">palakowan a palakowan ta tikelrowan,</FORM>
        <TRANSL xml:lang="zh">聚會所的鞦韆,</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229965.mp3" url="https://web.klokah.tw/text/sound//17646/229965.mp3"/>
    </S>
    <S id="229977">
        <FORM kindOf="original">ra'emec mi na ranuv.</FORM>
        <FORM kindOf="standard">ra'emec mi na ranov.</FORM>
        <TRANSL xml:lang="zh">我們小夥伴最認真。</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229977.mp3" url="https://web.klokah.tw/text/sound//17647/229977.mp3"/>
    </S>
    <S id="229978">
        <FORM kindOf="original">ma'udalr i pacalan adi kurselev na maymay kiwmalr i unar kani mumu"man adi kurselev na maymay adi ka talrunger?"</FORM>
        <FORM kindOf="standard">ma'odalr i pacalan adi korselev na maymay kiwmalr i onar kani momo"man adi korselev na maymay adi ka talronger?"</FORM>
        <TRANSL xml:lang="zh">外面下著毛毛雨,鴨子從來不躲雨。 弟弟問祖母:「鴨子為何不躲雨,鴨子不怕淋濕嗎?」</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229978.mp3" url="https://web.klokah.tw/text/sound//17648/229978.mp3"/>
    </S>
    <S id="229975">
        <FORM kindOf="original">pavekas mi kipakarun,</FORM>
        <FORM kindOf="standard">pavekas mi kipakaron,</FORM>
        <TRANSL xml:lang="zh">被使喚時跑最快,</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229975.mp3" url="https://web.klokah.tw/text/sound//17647/229975.mp3"/>
    </S>
    <S id="229985">
        <FORM kindOf="original">semangal ta misa'ur semenay,</FORM>
        <FORM kindOf="standard">semangal ta misa'or semenay,</FORM>
        <TRANSL xml:lang="zh">大家歡喜唱古調,</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229985.mp3" url="https://web.klokah.tw/text/sound//17649/229985.mp3"/>
    </S>
    <S id="229945">
        <FORM kindOf="original">'inavaya vulaya mi.</FORM>
        <FORM kindOf="standard">'inavaya volaya mi.</FORM>
        <TRANSL xml:lang="zh">讓我們美麗、讓我們好好。</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229945.mp3" url="https://web.klokah.tw/text/sound//17642/229945.mp3"/>
    </S>
    <S id="229986">
        <FORM kindOf="original">matemuy tu paliding na ma'inayan,</FORM>
        <FORM kindOf="standard">matemoy to paliding na ma'inayan,</FORM>
        <TRANSL xml:lang="zh">男人車車滿載著,</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229986.mp3" url="https://web.klokah.tw/text/sound//17649/229986.mp3"/>
    </S>
    <S id="229970">
        <FORM kindOf="original">pinuyumayan ta.</FORM>
        <FORM kindOf="standard">pinoyomayan ta.</FORM>
        <TRANSL xml:lang="zh">我們是卑南族。</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229970.mp3" url="https://web.klokah.tw/text/sound//17647/229970.mp3"/>
    </S>
    <S id="229971">
        <FORM kindOf="original">inu mu Puyuma,</FORM>
        <FORM kindOf="standard">ino mo Poyoma,</FORM>
        <TRANSL xml:lang="zh">你是南王的,</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229971.mp3" url="https://web.klokah.tw/text/sound//17647/229971.mp3"/>
    </S>
    <S id="229973">
        <FORM kindOf="original">ingku mu Kasavakan,</FORM>
        <FORM kindOf="standard">ingko mo Kasavakan,</FORM>
        <TRANSL xml:lang="zh">我是建和的,</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229973.mp3" url="https://web.klokah.tw/text/sound//17647/229973.mp3"/>
    </S>
    <S id="229974">
        <FORM kindOf="original">nu kalratudan mu,</FORM>
        <FORM kindOf="standard">no kalratodan mo,</FORM>
        <TRANSL xml:lang="zh">在除小米草時,</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229974.mp3" url="https://web.klokah.tw/text/sound//17647/229974.mp3"/>
    </S>
    <S id="229982">
        <FORM kindOf="original">yawa ki'apuca na rima'enan,</FORM>
        <FORM kindOf="standard">yawa ki'apoca na rima'enan,</FORM>
        <TRANSL xml:lang="zh">親友正要忙採菊,</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229982.mp3" url="https://web.klokah.tw/text/sound//17649/229982.mp3"/>
    </S>
    <S id="229979">
        <FORM kindOf="original">riked i mumu tumuvang "mipukapuk na maymay kemawan da mikappa na pukapuk".</FORM>
        <FORM kindOf="standard">riked i momo tomovang "mipokapok na maymay kemawan da mikappa na pokapok".</FORM>
        <TRANSL xml:lang="zh">祖母笑著說:「因為鴨子有羽毛,羽毛就像雨衣啊!」</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229979.mp3" url="https://web.klokah.tw/text/sound//17648/229979.mp3"/>
    </S>
    <S id="229980">
        <FORM kindOf="original">kiyang, kiyang, kiyang!</FORM>
        <FORM kindOf="standard">kiyang, kiyang, kiyang!</FORM>
        <TRANSL xml:lang="zh">鏘!鏘!鏘!</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229980.mp3" url="https://web.klokah.tw/text/sound//17649/229980.mp3"/>
    </S>
    <S id="229981">
        <FORM kindOf="original">pacungangcung na vulavulayan,</FORM>
        <FORM kindOf="standard">pacongangcong na volavolayan,</FORM>
        <TRANSL xml:lang="zh">姑娘忙敲報訊鈴,</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229981.mp3" url="https://web.klokah.tw/text/sound//17649/229981.mp3"/>
    </S>
    <S id="229983">
        <FORM kindOf="original">'ilrepat papiya na tainayan,</FORM>
        <FORM kindOf="standard">'ilrepat papiya na tainayan,</FORM>
        <TRANSL xml:lang="zh">個個都是巧手師,</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229983.mp3" url="https://web.klokah.tw/text/sound//17649/229983.mp3"/>
    </S>
    <S id="229976">
        <FORM kindOf="original">ranuv kinakuwa mi,</FORM>
        <FORM kindOf="standard">ranov kinakowa mi,</FORM>
        <TRANSL xml:lang="zh">我們被稱小夥伴,</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229976.mp3" url="https://web.klokah.tw/text/sound//17647/229976.mp3"/>
    </S>
    <S id="229989">
        <FORM kindOf="original">ulra na vali di runev,</FORM>
        <FORM kindOf="standard">olra na vali di ronev,</FORM>
        <TRANSL xml:lang="zh">有風和雲,</TRANSL>
        <AUDIO file="圖畫故事篇_Jianhe_Puyuma_229989.mp3" url="https://web.klokah.tw/text/sound//17650/229989.mp3"/>
    </S>
    <S id="229987">
        <FORM kindOf="original">yawa pacapaya i ama da ta 'apuc.</FORM>
        <FORM kindOf="standard">yawa pacapaya i ama da ta 'apoc.</FORM>
        <TRANSL xml:lang="zh">爸爸烘烤菊花茶。</TRANSL>
