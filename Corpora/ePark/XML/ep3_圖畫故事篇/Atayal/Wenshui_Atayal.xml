<TEXT id="eP3_圖畫故事篇_Wenshui_Atayal" xml:lang="tay" source="ePark3 圖畫故事篇 Wenshui_Atayal" audio="diarized" copyright="CC-BY-NC-SA" citation="Indigenous Languages Research and Development Foundation. (2020). 族語E樂園. https://web.klokah.tw/" BibTeX_citation="@misc{ePark, author = {Indigenous Languages Research and Development Foundation}, title = {族語E樂園}, year = {2020}, url = {https://web.klokah.tw/} }">
    <S id="230469">
        <FORM>wagi'! wagi'! wagi'!</FORM>
        <TRANSL xml:lang="zh">太陽!太陽!太陽!</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230469.mp3" url="https://web.klokah.tw/text/sound//17694/230469.mp3"/>
    </S>
    <S id="230470">
        <FORM>buun na' Itaal ku' wagi' ka' qutux.</FORM>
        <TRANSL xml:lang="zh">泰雅人射下一個太陽，</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230470.mp3" url="https://web.klokah.tw/text/sound//17694/230470.mp3"/>
    </S>
    <S id="230471">
        <FORM>manaaring 'i' kiya' la ga,</FORM>
        <TRANSL xml:lang="zh">從此以後，</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230471.mp3" url="https://web.klokah.tw/text/sound//17694/230471.mp3"/>
    </S>
    <S id="230472">
        <FORM>kiya' a' gabiyan la.</FORM>
        <TRANSL xml:lang="zh">有了晚上。</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230472.mp3" url="https://web.klokah.tw/text/sound//17694/230472.mp3"/>
    </S>
    <S id="230482">
        <FORM>amil ru qabubing ku' hani.</FORM>
        <TRANSL xml:lang="zh">這是鞋子和帽子。</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230482.mp3" url="https://web.klokah.tw/text/sound//17696/230482.mp3"/>
    </S>
    <S id="230486">
        <FORM>cu' ibabawiq tikay! cu' ibabawiq tikay!</FORM>
        <TRANSL xml:lang="zh">再高一點，再高一點!</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230486.mp3" url="https://web.klokah.tw/text/sound//17698/230486.mp3"/>
    </S>
    <S id="230487">
        <FORM>gumlug ci' cku' hangaliq ni' yaba'.</FORM>
        <TRANSL xml:lang="zh">騎在爸爸的肩膀上。</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230487.mp3" url="https://web.klokah.tw/text/sound//17698/230487.mp3"/>
    </S>
    <S id="230488">
        <FORM>mashahawr ru sipanga cu' ni' yaba' la.</FORM>
        <TRANSL xml:lang="zh">溜下來讓爸爸背我。</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230488.mp3" url="https://web.klokah.tw/text/sound//17698/230488.mp3"/>
    </S>
    <S id="230477">
        <FORM>nanuwan ku' hani?</FORM>
        <TRANSL xml:lang="zh">這是什麼?</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230477.mp3" url="https://web.klokah.tw/text/sound//17696/230477.mp3"/>
    </S>
    <S id="230478">
        <FORM>siyatu' ku' hani.</FORM>
        <TRANSL xml:lang="zh">這是衣服。</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230478.mp3" url="https://web.klokah.tw/text/sound//17696/230478.mp3"/>
    </S>
    <S id="230496">
        <FORM>tuspa' cku' gaa' nku' kanayril ku' wangri'.</FORM>
        <TRANSL xml:lang="zh">蒼蠅停留在女人大腿內側。</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230496.mp3" url="https://web.klokah.tw/text/sound//17700/230496.mp3"/>
    </S>
    <S id="230480">
        <FORM>awpun ku' hani.</FORM>
        <TRANSL xml:lang="zh">這是褲子。</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230480.mp3" url="https://web.klokah.tw/text/sound//17696/230480.mp3"/>
    </S>
    <S id="230481">
        <FORM>nanuwan ku' hani?</FORM>
        <TRANSL xml:lang="zh">這是什麼?</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230481.mp3" url="https://web.klokah.tw/text/sound//17696/230481.mp3"/>
    </S>
    <S id="230483">
        <FORM>ibabawiq ru mamawasiq ku' ragiyax.</FORM>
        <TRANSL xml:lang="zh">高聳而蒼翠的山脈，</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230483.mp3" url="https://web.klokah.tw/text/sound//17697/230483.mp3"/>
    </S>
    <S id="230500">
        <FORM>pacpung cu' lalihbun na' Itaal ku' hakri' na' utux.</FORM>
        <TRANSL xml:lang="zh">由神靈橋裁定泰雅人的良善。</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230500.mp3" url="https://web.klokah.tw/text/sound//17701/230500.mp3"/>
    </S>
    <S id="230501">
        <FORM>ini ta' manahmuhmut ga,</FORM>
        <TRANSL xml:lang="zh">不要胡作非為，</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230501.mp3" url="https://web.klokah.tw/text/sound//17701/230501.mp3"/>
    </S>
    <S id="230505">
        <FORM>ci! ci! ci! ci! 'i' mingaq.</FORM>
        <TRANSL xml:lang="zh">吱吱吱吱地叫，</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230505.mp3" url="https://web.klokah.tw/text/sound//17702/230505.mp3"/>
    </S>
    <S id="230490">
        <FORM>manbahag! manbahag!</FORM>
        <TRANSL xml:lang="zh">飛呀!飛呀!</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230490.mp3" url="https://web.klokah.tw/text/sound//17698/230490.mp3"/>
    </S>
    <S id="230495">
        <FORM>makahtu' cku' batunux. ku' rarusa' ka' maliku ru caxa' ka' kanayril.</FORM>
        <TRANSL xml:lang="zh">兩個男人和一個女人從石頭蹦出來，</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230495.mp3" url="https://web.klokah.tw/text/sound//17700/230495.mp3"/>
    </S>
    <S id="230508">
        <FORM>baq 'i' tuminun ku' kanayrill na' Itaal,</FORM>
        <TRANSL xml:lang="zh">泰雅女人會織布，</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230508.mp3" url="https://web.klokah.tw/text/sound//17703/230508.mp3"/>
    </S>
    <S id="230497">
        <FORM>manaaring 'i' kiya' la ga,</FORM>
        <TRANSL xml:lang="zh">此後，</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230497.mp3" url="https://web.klokah.tw/text/sound//17700/230497.mp3"/>
    </S>
    <S id="230510">
        <FORM>mapanga' cu' kagisi' 'i' yaki,</FORM>
        <TRANSL xml:lang="zh">祖母背著背簍，</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230510.mp3" url="https://web.klokah.tw/text/sound//17704/230510.mp3"/>
    </S>
    <S id="230511">
        <FORM>kiya' a' turakis cku' kagisi' niya',</FORM>
        <TRANSL xml:lang="zh">背簍裡有小米，</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230511.mp3" url="https://web.klokah.tw/text/sound//17704/230511.mp3"/>
    </S>
    <S id="230512">
        <FORM>kiya' a' singut cku' kagisi' niya',</FORM>
        <TRANSL xml:lang="zh">背簍裡有樹豆，</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230512.mp3" url="https://web.klokah.tw/text/sound//17704/230512.mp3"/>
    </S>
    <S id="230513">
        <FORM>balayq 'i' niqun ku' turakis ru singut.</FORM>
        <TRANSL xml:lang="zh">小米、樹豆好好吃。</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230513.mp3" url="https://web.klokah.tw/text/sound//17704/230513.mp3"/>
    </S>
    <S id="417004">
        <FORM>buwating! buwating!</FORM>
        <TRANSL xml:lang="zh">月亮!月亮!</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_417004.mp3" url="https://web.klokah.tw/text/sound//24068/417004.mp3"/>
    </S>
    <S id="417005">
        <FORM>matimurul ku' buwating la. matimurul cubalay!</FORM>
        <TRANSL xml:lang="zh">月圓了,好圓好圓!</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_417005.mp3" url="https://web.klokah.tw/text/sound//24068/417005.mp3"/>
    </S>
    <S id="417006">
        <FORM>matimurul ku' buwating cubalay.</FORM>
        <TRANSL xml:lang="zh">好圓好圓的月亮!</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_417006.mp3" url="https://web.klokah.tw/text/sound//24068/417006.mp3"/>
    </S>
    <S id="230503">
        <FORM>balayq nku' qawlit ku' makiya' cku' kug ka' ini salui.</FORM>
        <TRANSL xml:lang="zh">喜歡住在黑洞裡，</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230503.mp3" url="https://web.klokah.tw/text/sound//17702/230503.mp3"/>
    </S>
    <S id="230504">
        <FORM>gabiyan ga, muwah 'i' qumuliq cu' naniqun.</FORM>
        <TRANSL xml:lang="zh">晚上出來偷吃食物，</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230504.mp3" url="https://web.klokah.tw/text/sound//17702/230504.mp3"/>
    </S>
    <S id="230489">
        <FORM>mataqaqawmug ru mnanangi' 'i' manahuqil 'i' yaba'.</FORM>
        <TRANSL xml:lang="zh">趴下來，嗯，死翹翹了。</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230489.mp3" url="https://web.klokah.tw/text/sound//17698/230489.mp3"/>
    </S>
    <S id="230507">
        <FORM>baq 'i' qumaluwap ku' mamaliku na' Itaal,</FORM>
        <TRANSL xml:lang="zh">泰雅男人會打獵，</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230507.mp3" url="https://web.klokah.tw/text/sound//17703/230507.mp3"/>
    </S>
    <S id="230479">
        <FORM>nanuwan ku' hani?</FORM>
        <TRANSL xml:lang="zh">這是什麼?</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230479.mp3" url="https://web.klokah.tw/text/sound//17696/230479.mp3"/>
    </S>
    <S id="230509">
        <FORM>balayq 'i' patiqan ku' raqinas naha' la.</FORM>
        <TRANSL xml:lang="zh">他們就可以紋面了。</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230509.mp3" url="https://web.klokah.tw/text/sound//17703/230509.mp3"/>
    </S>
    <S id="230498">
        <FORM>araruwa 'i' matagbahuwag ku' Itaal la.</FORM>
        <TRANSL xml:lang="zh">泰雅族就繁衍了後代。</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230498.mp3" url="https://web.klokah.tw/text/sound//17700/230498.mp3"/>
    </S>
    <S id="230499">
        <FORM>hakri' na' utux! hakri' na' utux!</FORM>
        <TRANSL xml:lang="zh">神靈之橋!神靈之橋!</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230499.mp3" url="https://web.klokah.tw/text/sound//17701/230499.mp3"/>
    </S>
    <S id="230484">
        <FORM>payux ku' kakahuniq ru kakakiman.</FORM>
        <TRANSL xml:lang="zh">很多樹木和草，</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230484.mp3" url="https://web.klokah.tw/text/sound//17697/230484.mp3"/>
    </S>
    <S id="230485">
        <FORM>Tabilas ku' ralu' nku' tinqalangan mu.</FORM>
        <TRANSL xml:lang="zh">我的名字叫圓墩。</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230485.mp3" url="https://web.klokah.tw/text/sound//17697/230485.mp3"/>
    </S>
    <S id="230502">
        <FORM>paphaynas ta' cku' hakri' na' utux.</FORM>
        <TRANSL xml:lang="zh">就可以通過神靈之橋。</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230502.mp3" url="https://web.klokah.tw/text/sound//17701/230502.mp3"/>
    </S>
    <S id="417007">
        <FORM>rahwal ku' buwating cubalay.</FORM>
        <TRANSL xml:lang="zh">月亮好大唷!</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_417007.mp3" url="https://web.klokah.tw/text/sound//24068/417007.mp3"/>
    </S>
    <S id="417008">
        <FORM>tali'! gumlug ikuwing ku' buwating.</FORM>
        <TRANSL xml:lang="zh">看,月亮一直跟著我!</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_417008.mp3" url="https://web.klokah.tw/text/sound//24068/417008.mp3"/>
    </S>
    <S id="417009">
        <FORM>buwating, paqilaap ci' la. mapasugagay ti' la.</FORM>
        <TRANSL xml:lang="zh">月亮,我要睡覺了。晚安!</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_417009.mp3" url="https://web.klokah.tw/text/sound//24068/417009.mp3"/>
    </S>
    <S id="230506">
        <FORM>sumtatunux ru ini cami qilaap kahabag.</FORM>
        <TRANSL xml:lang="zh">吵得我們睡不著覺。</TRANSL>
        <AUDIO file="圖畫故事篇_Wenshui_Atayal_230506.mp3" url="https://web.klokah.tw/text/sound//17702/230506.mp3"/>
    </S>
</TEXT>
