<TEXT id="eP3_圖畫故事篇_FourSeasons_Atayal" xml:lang="tay" source="ePark3 圖畫故事篇 FourSeasons_Atayal" audio="diarized" copyright="CC-BY-NC-SA" citation="Indigenous Languages Research and Development Foundation. (2020). 族語E樂園. https://web.klokah.tw/" BibTeX_citation="@misc{ePark, author = {Indigenous Languages Research and Development Foundation}, title = {族語E樂園}, year = {2020}, url = {https://web.klokah.tw/} }" glottocode="" dialect="Four Seasons">
    <S id="250703">
        <FORM kindOf="original">maniq la,</FORM>
        <FORM kindOf="standard">maniq la,</FORM>
        <TRANSL xml:lang="zh">吃飯了!</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250703.mp3" url="https://web.klokah.tw/text/sound//18074/250703.mp3"/>
    </S>
    <S id="250704">
        <FORM kindOf="original">cyan ngahi ryax coni,</FORM>
        <FORM kindOf="standard">cyan ngahi ryax coni,</FORM>
        <TRANSL xml:lang="zh">今天有地瓜，</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250704.mp3" url="https://web.klokah.tw/text/sound//18074/250704.mp3"/>
    </S>
    <S id="250705">
        <FORM kindOf="original">cyan ayang yahux sbalay uyi,</FORM>
        <FORM kindOf="standard">cyan ayang yahox sbalay oyi,</FORM>
        <TRANSL xml:lang="zh">也有龍葵湯，</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250705.mp3" url="https://web.klokah.tw/text/sound//18074/250705.mp3"/>
    </S>
    <S id="250695">
        <FORM kindOf="original">tuliq la!</FORM>
        <FORM kindOf="standard">toliq la!</FORM>
        <TRANSL xml:lang="zh">起來了!</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250695.mp3" url="https://web.klokah.tw/text/sound//18072/250695.mp3"/>
    </S>
    <S id="250710">
        <FORM kindOf="original">sgagay ta.</FORM>
        <FORM kindOf="standard">sgagay ta.</FORM>
        <TRANSL xml:lang="zh">我們再見了。</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250710.mp3" url="https://web.klokah.tw/text/sound//18075/250710.mp3"/>
    </S>
    <S id="250711">
        <FORM kindOf="original">ima isu?</FORM>
        <FORM kindOf="standard">ima iso?</FORM>
        <TRANSL xml:lang="zh">你是誰?</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250711.mp3" url="https://web.klokah.tw/text/sound//18076/250711.mp3"/>
    </S>
    <S id="250698">
        <FORM kindOf="original">pkusiy lukus su uyi.</FORM>
        <FORM kindOf="standard">pkosiy lokos so oyi.</FORM>
        <TRANSL xml:lang="zh">也穿上你的衣服吧。</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250698.mp3" url="https://web.klokah.tw/text/sound//18072/250698.mp3"/>
    </S>
    <S id="250715">
        <FORM kindOf="original">ita qabax ga cquliq na Tayal.</FORM>
        <FORM kindOf="standard">ita qabax ga cqoliq na Tayal.</FORM>
        <TRANSL xml:lang="zh">我們大家是泰雅人。</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250715.mp3" url="https://web.klokah.tw/text/sound//18076/250715.mp3"/>
    </S>
    <S id="250706">
        <FORM kindOf="original">blaq niqun calay.</FORM>
        <FORM kindOf="standard">blaq niqon calay.</FORM>
        <TRANSL xml:lang="zh">真是好吃。</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250706.mp3" url="https://web.klokah.tw/text/sound//18074/250706.mp3"/>
    </S>
    <S id="250707">
        <FORM kindOf="original">yaya, musa cu mqas biru la,</FORM>
        <FORM kindOf="standard">yaya, mosa co mqas biro la,</FORM>
        <TRANSL xml:lang="zh">媽媽!我要去讀書，</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250707.mp3" url="https://web.klokah.tw/text/sound//18075/250707.mp3"/>
    </S>
    <S id="250708">
        <FORM kindOf="original">blequn mu calay pqbaq ke na Tayal,</FORM>
        <FORM kindOf="standard">bleqon mo calay pqbaq ke na Tayal,</FORM>
        <TRANSL xml:lang="zh">我會好好的學習泰雅語，</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250708.mp3" url="https://web.klokah.tw/text/sound//18075/250708.mp3"/>
    </S>
    <S id="250891">
        <FORM kindOf="original">maki rruma uyi,</FORM>
        <FORM kindOf="standard">maki rroma oyi,</FORM>
        <TRANSL xml:lang="zh">也有竹林，</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250891.mp3" url="https://web.klokah.tw/text/sound//18079/250891.mp3"/>
    </S>
    <S id="250697">
        <FORM kindOf="original">pima rqenas su,</FORM>
        <FORM kindOf="standard">pima rqenas so,</FORM>
        <TRANSL xml:lang="zh">洗洗你的臉，</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250697.mp3" url="https://web.klokah.tw/text/sound//18072/250697.mp3"/>
    </S>
    <S id="250712">
        <FORM kindOf="original">kingan ga Maray • Moqu,</FORM>
        <FORM kindOf="standard">kingan ga Maray • Moqo,</FORM>
        <TRANSL xml:lang="zh">我是馬賴磨固，</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250712.mp3" url="https://web.klokah.tw/text/sound//18076/250712.mp3"/>
    </S>
    <S id="250713">
        <FORM kindOf="original">isu ya?</FORM>
        <FORM kindOf="standard">iso ya?</FORM>
        <TRANSL xml:lang="zh">你呢?</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250713.mp3" url="https://web.klokah.tw/text/sound//18076/250713.mp3"/>
    </S>
    <S id="250714">
        <FORM kindOf="original">Yaway • Watan qa kingan,</FORM>
        <FORM kindOf="standard">Yaway • Watan qa kingan,</FORM>
        <TRANSL xml:lang="zh">我是雅外瓦旦，</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250714.mp3" url="https://web.klokah.tw/text/sound//18076/250714.mp3"/>
    </S>
    <S id="250898">
        <FORM kindOf="original">saying wagi cyan maki babaw kayal,</FORM>
        <FORM kindOf="standard">saying wagi cyan maki babaw kayal,</FORM>
        <TRANSL xml:lang="zh">天上有兩個太陽，</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250898.mp3" url="https://web.klokah.tw/text/sound//18081/250898.mp3"/>
    </S>
    <S id="250899">
        <FORM kindOf="original">mu' qutux wagi cquliq Tayal,</FORM>
        <FORM kindOf="standard">mo' qotox wagi cqoliq Tayal,</FORM>
        <TRANSL xml:lang="zh">泰雅人射一個太陽，</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250899.mp3" url="https://web.klokah.tw/text/sound//18081/250899.mp3"/>
    </S>
    <S id="250900">
        <FORM kindOf="original">babaw nya laga, maki qlyan ro gbyan,</FORM>
        <FORM kindOf="standard">babaw nya laga, maki qlyan ro gbyan,</FORM>
        <TRANSL xml:lang="zh">之後，有白天和晚上，</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250900.mp3" url="https://web.klokah.tw/text/sound//18081/250900.mp3"/>
    </S>
    <S id="250709">
        <FORM kindOf="original">yunga nya, pkayan cu ke Tayal qa isu,</FORM>
        <FORM kindOf="standard">yonga nya, pkayan co ke Tayal qa iso,</FORM>
        <TRANSL xml:lang="zh">將來跟你說泰雅語，</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250709.mp3" url="https://web.klokah.tw/text/sound//18075/250709.mp3"/>
    </S>
    <S id="250902">
        <FORM kindOf="original">mlikuy ga baq qmalup ru cminun qwani,</FORM>
        <FORM kindOf="standard">mlikoy ga baq qmalop ro cminon qwani,</FORM>
        <TRANSL xml:lang="zh">男人會打獵和藤編，</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250902.mp3" url="https://web.klokah.tw/text/sound//18082/250902.mp3"/>
    </S>
    <S id="250889">
        <FORM kindOf="original">mqas calay qutux muyax myan qabax.</FORM>
        <FORM kindOf="standard">mqas calay qotox moyax myan qabax.</FORM>
        <TRANSL xml:lang="zh">我們一家人全部都很快樂。</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250889.mp3" url="https://web.klokah.tw/text/sound//18078/250889.mp3"/>
    </S>
    <S id="250893">
        <FORM kindOf="original">siyan mu calay qalang mu.</FORM>
        <FORM kindOf="standard">siyan mo calay qalang mo.</FORM>
        <TRANSL xml:lang="zh">我喜歡我們的部落。</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250893.mp3" url="https://web.klokah.tw/text/sound//18079/250893.mp3"/>
    </S>
    <S id="250895">
        <FORM kindOf="original">mbka laga,</FORM>
        <FORM kindOf="standard">mbka laga,</FORM>
        <TRANSL xml:lang="zh">破裂的時候，</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250895.mp3" url="https://web.klokah.tw/text/sound//18080/250895.mp3"/>
    </S>
    <S id="250896">
        <FORM kindOf="original">wayal mhtux kya cquliq,</FORM>
        <FORM kindOf="standard">wayal mhtox kya cqoliq,</FORM>
        <TRANSL xml:lang="zh">人就走出來，</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250896.mp3" url="https://web.klokah.tw/text/sound//18080/250896.mp3"/>
    </S>
    <S id="250887">
        <FORM kindOf="original">maki qsuyal mu mlikuy ro knerin,</FORM>
        <FORM kindOf="standard">maki qsoyal mo mlikoy ro knerin,</FORM>
        <TRANSL xml:lang="zh">有哥哥、姐姐，</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250887.mp3" url="https://web.klokah.tw/text/sound//18078/250887.mp3"/>
    </S>
    <S id="250696">
        <FORM kindOf="original">kugus pniq su,</FORM>
        <FORM kindOf="standard">kogos pniq so,</FORM>
        <TRANSL xml:lang="zh">刷刷你的牙齒，</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250696.mp3" url="https://web.klokah.tw/text/sound//18072/250696.mp3"/>
    </S>
    <S id="250894">
        <FORM kindOf="original">maki qutux tyaba na btunux,</FORM>
        <FORM kindOf="standard">maki qotox tyaba na btonox,</FORM>
        <TRANSL xml:lang="zh">有一塊好大的巨石，</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250894.mp3" url="https://web.klokah.tw/text/sound//18080/250894.mp3"/>
    </S>
    <S id="250903">
        <FORM kindOf="original">knerin ga baq cminun pala,</FORM>
        <FORM kindOf="standard">knerin ga baq cminon pala,</FORM>
        <TRANSL xml:lang="zh">女人會織布，</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250903.mp3" url="https://web.klokah.tw/text/sound//18082/250903.mp3"/>
    </S>
    <S id="250904">
        <FORM kindOf="original">ro mlahang muyax uyi,</FORM>
        <FORM kindOf="standard">ro mlahang moyax oyi,</FORM>
        <TRANSL xml:lang="zh">也要照顧家人，</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250904.mp3" url="https://web.klokah.tw/text/sound//18082/250904.mp3"/>
    </S>
    <S id="250892">
        <FORM kindOf="original">piyux rramat pnuya qmayah kya,</FORM>
        <FORM kindOf="standard">piyox rramat pnoya qmayah kya,</FORM>
        <TRANSL xml:lang="zh">田園種了很多蔬菜，</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250892.mp3" url="https://web.klokah.tw/text/sound//18079/250892.mp3"/>
    </S>
    <S id="250886">
        <FORM kindOf="original">maki yutas, yaki, yaba, yaya mu,</FORM>
        <FORM kindOf="standard">maki yotas, yaki, yaba, yaya mo,</FORM>
        <TRANSL xml:lang="zh">我有爺爺、奶奶、爸爸和媽媽，</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250886.mp3" url="https://web.klokah.tw/text/sound//18078/250886.mp3"/>
    </S>
    <S id="250897">
        <FORM kindOf="original">naha ga bnhici na ita Tayal.</FORM>
        <FORM kindOf="standard">naha ga bnhici na ita Tayal.</FORM>
        <TRANSL xml:lang="zh">他們就是我們泰雅族的祖先。</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250897.mp3" url="https://web.klokah.tw/text/sound//18080/250897.mp3"/>
    </S>
    <S id="250890">
        <FORM kindOf="original">maki rgyax, maki llyung qalang mu,</FORM>
        <FORM kindOf="standard">maki rgyax, maki llyong qalang mo,</FORM>
        <TRANSL xml:lang="zh">我的部落有山、有河流，</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250890.mp3" url="https://web.klokah.tw/text/sound//18079/250890.mp3"/>
    </S>
    <S id="250905">
        <FORM kindOf="original">mrkyas laga baqun matas rqenas.</FORM>
        <FORM kindOf="standard">mrkyas laga baqon matas rqenas.</FORM>
        <TRANSL xml:lang="zh">成人就可以紋面。</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250905.mp3" url="https://web.klokah.tw/text/sound//18082/250905.mp3"/>
    </S>
    <S id="250901">
        <FORM kindOf="original">gbyan ga ktalan byaling uyi.</FORM>
        <FORM kindOf="standard">gbyan ga ktalan byaling oyi.</FORM>
        <TRANSL xml:lang="zh">夜裡也能看到月亮。</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250901.mp3" url="https://web.klokah.tw/text/sound//18081/250901.mp3"/>
    </S>
    <S id="250888">
        <FORM kindOf="original">maki swahi mu mlikuy ro knerin uyi,</FORM>
        <FORM kindOf="standard">maki swahi mo mlikoy ro knerin oyi,</FORM>
        <TRANSL xml:lang="zh">也有弟弟、妹妹，</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250888.mp3" url="https://web.klokah.tw/text/sound//18078/250888.mp3"/>
    </S>
    <S id="250907">
        <FORM kindOf="original">huyay ta magal pinnqas na utux bnheci,</FORM>
        <FORM kindOf="standard">hoyay ta magal pinnqas na otox bnheci,</FORM>
        <TRANSL xml:lang="zh">才能獲得祖先的歡喜，</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250907.mp3" url="https://web.klokah.tw/text/sound//18083/250907.mp3"/>
    </S>
    <S id="250909">
        <FORM kindOf="original">iyat ta hotaw la.</FORM>
        <FORM kindOf="standard">iyat ta hotaw la.</FORM>
        <TRANSL xml:lang="zh">就不會掉下來。</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250909.mp3" url="https://web.klokah.tw/text/sound//18083/250909.mp3"/>
    </S>
    <S id="250908">
        <FORM kindOf="original">hkangi ta hongu utux bnheci laga,</FORM>
        <FORM kindOf="standard">hkangi ta hongo otox bnheci laga,</FORM>
        <TRANSL xml:lang="zh">我們走彩虹橋的時候，</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250908.mp3" url="https://web.klokah.tw/text/sound//18083/250908.mp3"/>
    </S>
    <S id="250906">
        <FORM kindOf="original">ptyawaw ta blaq na yaw,</FORM>
        <FORM kindOf="standard">ptyawaw ta blaq na yaw,</FORM>
        <TRANSL xml:lang="zh">我們來作好事，</TRANSL>
        <AUDIO file="圖畫故事篇_FourSeasons_Atayal_250906.mp3" url="https://web.klokah.tw/text/sound//18083/250906.mp3"/>
    </S>
</TEXT>