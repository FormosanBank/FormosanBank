<TEXT id="eP3_圖畫故事篇_YilanZeaol_Atayal" xml:lang="tay" source="ePark3 圖畫故事篇 YilanZeaol_Atayal" audio="diarized" copyright="CC-BY-NC-SA" citation="Indigenous Languages Research and Development Foundation. (2020). 族語E樂園. https://web.klokah.tw/" BibTeX_citation="@misc{ePark, author = {Indigenous Languages Research and Development Foundation}, title = {族語E樂園}, year = {2020}, url = {https://web.klokah.tw/} }" glottocode="" dialect="Yilan Zeaol">
    <S id="417308">
        <FORM kindOf="original">sgus g'nux,</FORM>
        <FORM kindOf="standard">sgos g'nox,</FORM>
        <TRANSL xml:lang="zh">刷刷牙,</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417308.mp3" url="https://web.klokah.tw/text/sound//25184/417308.mp3"/>
    </S>
    <S id="417368">
        <FORM kindOf="original">yaya, mosa cu m'was biru la,</FORM>
        <FORM kindOf="standard">yaya, mosa co m'was biro la,</FORM>
        <TRANSL xml:lang="zh">媽媽!我要去上學了,</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417368.mp3" url="https://web.klokah.tw/text/sound//25188/417368.mp3"/>
    </S>
    <S id="417331">
        <FORM kindOf="original">ble ci ni'un.</FORM>
        <FORM kindOf="standard">ble ci ni'on.</FORM>
        <TRANSL xml:lang="zh">很好吃。</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417331.mp3" url="https://web.klokah.tw/text/sound//25186/417331.mp3"/>
    </S>
    <S id="417369">
        <FORM kindOf="original">tkba' cu ke' Tayal,</FORM>
        <FORM kindOf="standard">tkba' co ke' Tayal,</FORM>
        <TRANSL xml:lang="zh">我要學習泰雅語,</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417369.mp3" url="https://web.klokah.tw/text/sound//25188/417369.mp3"/>
    </S>
    <S id="417370">
        <FORM kindOf="original">pkayan ta ke' Tayal babaw nya,</FORM>
        <FORM kindOf="standard">pkayan ta ke' Tayal babaw nya,</FORM>
        <TRANSL xml:lang="zh">將來我們一起說泰雅語,</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417370.mp3" url="https://web.klokah.tw/text/sound//25188/417370.mp3"/>
    </S>
    <S id="417329">
        <FORM kindOf="original">maki ngahi,</FORM>
        <FORM kindOf="standard">maki ngahi,</FORM>
        <TRANSL xml:lang="zh">有地瓜,</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417329.mp3" url="https://web.klokah.tw/text/sound//25186/417329.mp3"/>
    </S>
    <S id="417378">
        <FORM kindOf="original">SimaㆍHayung lalu mu,</FORM>
        <FORM kindOf="standard">SimaㆍHayong lalo mo,</FORM>
        <TRANSL xml:lang="zh">我叫希瑪·哈勇,</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417378.mp3" url="https://web.klokah.tw/text/sound//25189/417378.mp3"/>
    </S>
    <S id="417388">
        <FORM kindOf="original">sli mu calay alang mu.</FORM>
        <FORM kindOf="standard">sli mo calay alang mo.</FORM>
        <TRANSL xml:lang="zh">我很喜歡我的部落。</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417388.mp3" url="https://web.klokah.tw/text/sound//25191/417388.mp3"/>
    </S>
    <S id="417380">
        <FORM kindOf="original">utux sali mu ga,</FORM>
        <FORM kindOf="standard">otox sali mo ga,</FORM>
        <TRANSL xml:lang="zh">我的一家人,</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417380.mp3" url="https://web.klokah.tw/text/sound//25190/417380.mp3"/>
    </S>
    <S id="417377">
        <FORM kindOf="original">isu ga ?</FORM>
        <FORM kindOf="standard">iso ga ?</FORM>
        <TRANSL xml:lang="zh">你呢?</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417377.mp3" url="https://web.klokah.tw/text/sound//25189/417377.mp3"/>
    </S>
    <S id="417398">
        <FORM kindOf="original">utux ryax ga mcka sa ing,</FORM>
        <FORM kindOf="standard">otox ryax ga mcka sa ing,</FORM>
        <TRANSL xml:lang="zh">一天破裂為兩半,</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417398.mp3" url="https://web.klokah.tw/text/sound//25193/417398.mp3"/>
    </S>
    <S id="417383">
        <FORM kindOf="original">sa ing sswe mu nerin.</FORM>
        <FORM kindOf="standard">sa ing sswe mo nerin.</FORM>
        <TRANSL xml:lang="zh">二個妹妹。</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417383.mp3" url="https://web.klokah.tw/text/sound//25190/417383.mp3"/>
    </S>
    <S id="417410">
        <FORM kindOf="original">ptasan ta r'yas nku mlikuy Tayal.</FORM>
        <FORM kindOf="standard">ptasan ta r'yas nko mlikoy Tayal.</FORM>
        <TRANSL xml:lang="zh">我們就要為泰雅男子紋面。</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417410.mp3" url="https://web.klokah.tw/text/sound//25196/417410.mp3"/>
    </S>
    <S id="417375">
        <FORM kindOf="original">ima isu ?</FORM>
        <FORM kindOf="standard">ima iso ?</FORM>
        <TRANSL xml:lang="zh">你是誰?</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417375.mp3" url="https://web.klokah.tw/text/sound//25189/417375.mp3"/>
    </S>
    <S id="417330">
        <FORM kindOf="original">maki iyung wasi,</FORM>
        <FORM kindOf="standard">maki iyong wasi,</FORM>
        <TRANSL xml:lang="zh">有龍葵湯,</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417330.mp3" url="https://web.klokah.tw/text/sound//25186/417330.mp3"/>
    </S>
    <S id="417434">
        <FORM kindOf="original">myasa lungan ta,</FORM>
        <FORM kindOf="standard">myasa longan ta,</FORM>
        <TRANSL xml:lang="zh">心地善良,</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417434.mp3" url="https://web.klokah.tw/text/sound//25198/417434.mp3"/>
    </S>
    <S id="417435">
        <FORM kindOf="original">mtiyaw ta myasa yaw,</FORM>
        <FORM kindOf="standard">mtiyaw ta myasa yaw,</FORM>
        <TRANSL xml:lang="zh">作好事,</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417435.mp3" url="https://web.klokah.tw/text/sound//25198/417435.mp3"/>
    </S>
    <S id="417436">
        <FORM kindOf="original">karaw ta hongu lutux babaw nya,</FORM>
        <FORM kindOf="standard">karaw ta hongo lotox babaw nya,</FORM>
        <TRANSL xml:lang="zh">將来走上彩虹橋,</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417436.mp3" url="https://web.klokah.tw/text/sound//25198/417436.mp3"/>
    </S>
    <S id="417309">
        <FORM kindOf="original">mema r'yas,</FORM>
        <FORM kindOf="standard">mema r'yas,</FORM>
        <TRANSL xml:lang="zh">洗洗臉,</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417309.mp3" url="https://web.klokah.tw/text/sound//25184/417309.mp3"/>
    </S>
    <S id="417382">
        <FORM kindOf="original">utux suyan mu likuy,</FORM>
        <FORM kindOf="standard">otox soyan mo likoy,</FORM>
        <TRANSL xml:lang="zh">一個哥哥,</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417382.mp3" url="https://web.klokah.tw/text/sound//25190/417382.mp3"/>
    </S>
    <S id="417371">
        <FORM kindOf="original">sgagay ta la.</FORM>
        <FORM kindOf="standard">sgagay ta la.</FORM>
        <TRANSL xml:lang="zh">再見!</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417371.mp3" url="https://web.klokah.tw/text/sound//25188/417371.mp3"/>
    </S>
    <S id="417387">
        <FORM kindOf="original">piyux ci pagay ru rrami,</FORM>
        <FORM kindOf="standard">piyox ci pagay ro rrami,</FORM>
        <TRANSL xml:lang="zh">很多稻米和菜蔬,</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417387.mp3" url="https://web.klokah.tw/text/sound//25191/417387.mp3"/>
    </S>
    <S id="417385">
        <FORM kindOf="original">alang mu ga,</FORM>
        <FORM kindOf="standard">alang mo ga,</FORM>
        <TRANSL xml:lang="zh">我的部落,有山和河流,</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417385.mp3" url="https://web.klokah.tw/text/sound//25191/417385.mp3"/>
    </S>
    <S id="417404">
        <FORM kindOf="original">sa'ing wagi maki babaw kayan,</FORM>
        <FORM kindOf="standard">sa'ing wagi maki babaw kayan,</FORM>
        <TRANSL xml:lang="zh">天上有兩個太陽,</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417404.mp3" url="https://web.klokah.tw/text/sound//25195/417404.mp3"/>
    </S>
    <S id="417399">
        <FORM kindOf="original">kahun cka mhtuw c'oli,</FORM>
        <FORM kindOf="standard">kahon cka mhtow c'oli,</FORM>
        <TRANSL xml:lang="zh">裂石中間出現人,</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417399.mp3" url="https://web.klokah.tw/text/sound//25193/417399.mp3"/>
    </S>
    <S id="417400">
        <FORM kindOf="original">naha ga bnkisan ta Tayal.</FORM>
        <FORM kindOf="standard">naha ga bnkisan ta Tayal.</FORM>
        <TRANSL xml:lang="zh">他們就是我們泰雅族的祖先。</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417400.mp3" url="https://web.klokah.tw/text/sound//25193/417400.mp3"/>
    </S>
    <S id="417381">
        <FORM kindOf="original">maki uba, uya, yaba ru yaya,</FORM>
        <FORM kindOf="standard">maki oba, oya, yaba ro yaya,</FORM>
        <TRANSL xml:lang="zh">有爺爺、奶奶、爸爸和媽媽,</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417381.mp3" url="https://web.klokah.tw/text/sound//25190/417381.mp3"/>
    </S>
    <S id="417405">
        <FORM kindOf="original">kilux calay ru ungat gbyan,</FORM>
        <FORM kindOf="standard">kilox calay ro ongat gbyan,</FORM>
        <TRANSL xml:lang="zh">炎熱又没有晚上,</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417405.mp3" url="https://web.klokah.tw/text/sound//25195/417405.mp3"/>
    </S>
    <S id="417310">
        <FORM kindOf="original">cmi lukus.</FORM>
        <FORM kindOf="standard">cmi lokos.</FORM>
        <TRANSL xml:lang="zh">穿上衣服。</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417310.mp3" url="https://web.klokah.tw/text/sound//25184/417310.mp3"/>
    </S>
    <S id="417406">
        <FORM kindOf="original">cbun mlikuy Tayal utux wagi,</FORM>
        <FORM kindOf="standard">cbon mlikoy Tayal otox wagi,</FORM>
        <TRANSL xml:lang="zh">泰雅勇士射下一個太陽,</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417406.mp3" url="https://web.klokah.tw/text/sound//25195/417406.mp3"/>
    </S>
    <S id="417397">
        <FORM kindOf="original">mnaki utux tyaba btunux,</FORM>
        <FORM kindOf="standard">mnaki otox tyaba btonox,</FORM>
        <TRANSL xml:lang="zh">曾有一塊大巨石,</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417397.mp3" url="https://web.klokah.tw/text/sound//25193/417397.mp3"/>
    </S>
    <S id="417407">
        <FORM kindOf="original">mita ta byacing cka bengi.</FORM>
        <FORM kindOf="standard">mita ta byacing cka bengi.</FORM>
        <TRANSL xml:lang="zh">從此夜晚看到月亮。</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417407.mp3" url="https://web.klokah.tw/text/sound//25195/417407.mp3"/>
    </S>
    <S id="417409">
        <FORM kindOf="original">ba' magan sinu, ba' tminun wani Iga,</FORM>
        <FORM kindOf="standard">ba' magan sino, ba' tminon wani Iga,</FORM>
        <TRANSL xml:lang="zh">會打獵,會藤編後,</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417409.mp3" url="https://web.klokah.tw/text/sound//25196/417409.mp3"/>
    </S>
    <S id="417386">
        <FORM kindOf="original">maki rgyax ru llyung,maki ali ru 'cyux tayal,</FORM>
        <FORM kindOf="standard">maki rgyax ro llyong,maki ali ro 'cyox tayal,</FORM>
        <TRANSL xml:lang="zh">有竹简和苦花魚,</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417386.mp3" url="https://web.klokah.tw/text/sound//25191/417386.mp3"/>
    </S>
    <S id="417411">
        <FORM kindOf="original">ba' tminun pala, ba' mlahang sali Iga</FORM>
        <FORM kindOf="standard">ba' tminon pala, ba' mlahang sali Iga</FORM>
        <TRANSL xml:lang="zh">會織布,會照願家人後,</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417411.mp3" url="https://web.klokah.tw/text/sound//25196/417411.mp3"/>
    </S>
    <S id="417412">
        <FORM kindOf="original">ptasan ta r'yas nku nerin Tayal.</FORM>
        <FORM kindOf="standard">ptasan ta r'yas nko nerin Tayal.</FORM>
        <TRANSL xml:lang="zh">我們就要為泰雅女子紋面。</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417412.mp3" url="https://web.klokah.tw/text/sound//25196/417412.mp3"/>
    </S>
    <S id="417376">
        <FORM kindOf="original">lalu mu ga Yukanㆍ Wilang,</FORM>
        <FORM kindOf="standard">lalo mo ga Yokanㆍ Wilang,</FORM>
        <TRANSL xml:lang="zh">我叫尤幹·偉浪,</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417376.mp3" url="https://web.klokah.tw/text/sound//25189/417376.mp3"/>
    </S>
    <S id="417328">
        <FORM kindOf="original">mani la.</FORM>
        <FORM kindOf="standard">mani la.</FORM>
        <TRANSL xml:lang="zh">吃飯了!</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417328.mp3" url="https://web.klokah.tw/text/sound//25186/417328.mp3"/>
    </S>
    <S id="417307">
        <FORM kindOf="original">tuli la!</FORM>
        <FORM kindOf="standard">toli la!</FORM>
        <TRANSL xml:lang="zh">起床了!</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417307.mp3" url="https://web.klokah.tw/text/sound//25184/417307.mp3"/>
    </S>
    <S id="417437">
        <FORM kindOf="original">iyat ta shotaw,</FORM>
        <FORM kindOf="standard">iyat ta shotaw,</FORM>
        <TRANSL xml:lang="zh">我們不會掉下來,</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417437.mp3" url="https://web.klokah.tw/text/sound//25198/417437.mp3"/>
    </S>
    <S id="417438">
        <FORM kindOf="original">ble ta tehok alang bnkisan.</FORM>
        <FORM kindOf="standard">ble ta tehok alang bnkisan.</FORM>
        <TRANSL xml:lang="zh">平安到達祖盤的家郷</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417438.mp3" url="https://web.klokah.tw/text/sound//25198/417438.mp3"/>
    </S>
    <S id="417379">
        <FORM kindOf="original">Tayal ta kwara.</FORM>
        <FORM kindOf="standard">Tayal ta kwara.</FORM>
        <TRANSL xml:lang="zh">我們都是泰雅人。</TRANSL>
        <AUDIO file="圖畫故事篇_YilanZeaol_Atayal_417379.mp3" url="https://web.klokah.tw/text/sound//25189/417379.mp3"/>
    </S>
</TEXT>