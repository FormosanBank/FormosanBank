        <AUDIO file="圖畫故事篇_Sekolik_Atayal_229633.mp3" url="https://web.klokah.tw/text/sound//17609/229633.mp3"/>
    </S>
    <S id="229641">
        <FORM kindOf="original">kya singut kiri nya.</FORM>
        <FORM kindOf="standard">kya singot kiri nya.</FORM>
        <TRANSL xml:lang="zh">背簍裡有樹豆，</TRANSL>
        <AUDIO file="圖畫故事篇_Sekolik_Atayal_229641.mp3" url="https://web.klokah.tw/text/sound//17611/229641.mp3"/>
    </S>
    <S id="229653">
        <FORM kindOf="original">qoli ga qoli.</FORM>
        <FORM kindOf="standard">qoli ga qoli.</FORM>
        <TRANSL xml:lang="zh">老鼠呀老鼠。</TRANSL>
        <AUDIO file="圖畫故事篇_Sekolik_Atayal_229653.mp3" url="https://web.klokah.tw/text/sound//17614/229653.mp3"/>
    </S>
