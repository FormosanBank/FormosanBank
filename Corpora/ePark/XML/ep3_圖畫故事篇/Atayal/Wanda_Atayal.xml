<TEXT id="eP3_圖畫故事篇_Wanda_Atayal" xml:lang="tay" source="ePark3 圖畫故事篇 Wanda_Atayal" audio="diarized" copyright="CC-BY-NC-SA" citation="Indigenous Languages Research and Development Foundation. (2020). 族語E樂園. https://web.klokah.tw/" BibTeX_citation="@misc{ePark, author = {Indigenous Languages Research and Development Foundation}, title = {族語E樂園}, year = {2020}, url = {https://web.klokah.tw/} }" glottocode="" dialect="Wanda">
    <S id="230514">
        <FORM kindOf="original">wagi'! wagi'! wagi'!</FORM>
        <FORM kindOf="standard">wagi'! wagi'! wagi'!</FORM>
        <TRANSL xml:lang="zh">太陽!太陽!太陽!</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230514.mp3" url="https://web.klokah.tw/text/sound//17705/230514.mp3"/>
    </S>
    <S id="230515">
        <FORM kindOf="original">buu na Itaral ka utux wagi'.</FORM>
        <FORM kindOf="standard">boo na Itaral ka otox wagi'.</FORM>
        <TRANSL xml:lang="zh">泰雅人射下一個太陽，</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230515.mp3" url="https://web.klokah.tw/text/sound//17705/230515.mp3"/>
    </S>
    <S id="230516">
        <FORM kindOf="original">sa'aring ci kaca' la ga.</FORM>
        <FORM kindOf="standard">sa'aring ci kaca' la ga.</FORM>
        <TRANSL xml:lang="zh">從此以後，</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230516.mp3" url="https://web.klokah.tw/text/sound//17705/230516.mp3"/>
    </S>
    <S id="230517">
        <FORM kindOf="original">kelan na malahngan la.</FORM>
        <FORM kindOf="standard">kelan na malahngan la.</FORM>
        <TRANSL xml:lang="zh">有了晚上。</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230517.mp3" url="https://web.klokah.tw/text/sound//17705/230517.mp3"/>
    </S>
    <S id="230518">
        <FORM kindOf="original">amol ka hani'?</FORM>
        <FORM kindOf="standard">amol ka hani'?</FORM>
        <TRANSL xml:lang="zh">這是什麼?</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230518.mp3" url="https://web.klokah.tw/text/sound//17706/230518.mp3"/>
    </S>
    <S id="230519">
        <FORM kindOf="original">lukus ka hani'.</FORM>
        <FORM kindOf="standard">lokos ka hani'.</FORM>
        <TRANSL xml:lang="zh">這是衣服。</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230519.mp3" url="https://web.klokah.tw/text/sound//17706/230519.mp3"/>
    </S>
    <S id="230520">
        <FORM kindOf="original">amol ka hani'?</FORM>
        <FORM kindOf="standard">amol ka hani'?</FORM>
        <TRANSL xml:lang="zh">這是什麼?</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230520.mp3" url="https://web.klokah.tw/text/sound//17706/230520.mp3"/>
    </S>
    <S id="230522">
        <FORM kindOf="original">amol ka hani'?</FORM>
        <FORM kindOf="standard">amol ka hani'?</FORM>
        <TRANSL xml:lang="zh">這是什麼?</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230522.mp3" url="https://web.klokah.tw/text/sound//17706/230522.mp3"/>
    </S>
    <S id="230525">
        <FORM kindOf="original">payux la kahuni' lu kankaman.</FORM>
        <FORM kindOf="standard">payox la kahoni' lo kankaman.</FORM>
        <TRANSL xml:lang="zh">很多樹木和草，</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230525.mp3" url="https://web.klokah.tw/text/sound//17707/230525.mp3"/>
    </S>
    <S id="230526">
        <FORM kindOf="original">Sasi ka ralu' ni alang mu.</FORM>
        <FORM kindOf="standard">Sasi ka ralo' ni alang mo.</FORM>
        <TRANSL xml:lang="zh">我的名字叫親愛。</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230526.mp3" url="https://web.klokah.tw/text/sound//17707/230526.mp3"/>
    </S>
    <S id="230521">
        <FORM kindOf="original">ara' ka hani'.</FORM>
        <FORM kindOf="standard">ara' ka hani'.</FORM>
        <TRANSL xml:lang="zh">這是褲子。</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230521.mp3" url="https://web.klokah.tw/text/sound//17706/230521.mp3"/>
    </S>
    <S id="230527">
        <FORM kindOf="original">ka babawi' tikiy! ka babawi' tikiy!</FORM>
        <FORM kindOf="standard">ka babawi' tikiy! ka babawi' tikiy!</FORM>
        <TRANSL xml:lang="zh">高一點再高一點，</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230527.mp3" url="https://web.klokah.tw/text/sound//17708/230527.mp3"/>
    </S>
    <S id="230523">
        <FORM kindOf="original">sapit laha ci tamuku' ka hani'.</FORM>
        <FORM kindOf="standard">sapit laha ci tamoko' ka hani'.</FORM>
        <TRANSL xml:lang="zh">這是鞋子和帽子。</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230523.mp3" url="https://web.klokah.tw/text/sound//17706/230523.mp3"/>
    </S>
    <S id="230524">
        <FORM kindOf="original">babawi' lu magaragu' ka ragex.</FORM>
        <FORM kindOf="standard">babawi' lo magarago' ka ragex.</FORM>
        <TRANSL xml:lang="zh">高聳而蒼翠的山脈，</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230524.mp3" url="https://web.klokah.tw/text/sound//17707/230524.mp3"/>
    </S>
    <S id="230528">
        <FORM kindOf="original">tapanga' ci hangali' ni yaba'.</FORM>
        <FORM kindOf="standard">tapanga' ci hangali' ni yaba'.</FORM>
        <TRANSL xml:lang="zh">騎在爸爸的肩膀上。</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230528.mp3" url="https://web.klokah.tw/text/sound//17708/230528.mp3"/>
    </S>
    <S id="230530">
        <FORM kindOf="original">mananangi' cu marapax ma'uxil.</FORM>
        <FORM kindOf="standard">mananangi' co marapax ma'oxil.</FORM>
        <TRANSL xml:lang="zh">我趴在地上假裝死掉。</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230530.mp3" url="https://web.klokah.tw/text/sound//17708/230530.mp3"/>
    </S>
    <S id="230532">
        <FORM kindOf="original">pakaṟek ci ka uṟati' mararusa' malikuṟ lu utux ka kanel.</FORM>
        <FORM kindOf="standard">pakaṟek ci ka oṟati' mararosa' malikoṟ lo otox ka kanel.</FORM>
        <TRANSL xml:lang="zh">兩個男人和一個女人從石頭蹦出來，</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230532.mp3" url="https://web.klokah.tw/text/sound//17709/230532.mp3"/>
    </S>
    <S id="230533">
        <FORM kindOf="original">taspa' ci ka muṟi' naka kanel ka ṟanglit.</FORM>
        <FORM kindOf="standard">taspa' ci ka moṟi' naka kanel ka ṟanglit.</FORM>
        <TRANSL xml:lang="zh">蒼蠅停留在女人大腿內側。</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230533.mp3" url="https://web.klokah.tw/text/sound//17709/230533.mp3"/>
    </S>
    <S id="230534">
        <FORM kindOf="original">sa'aring ki' la ga,</FORM>
        <FORM kindOf="standard">sa'aring ki' la ga,</FORM>
        <TRANSL xml:lang="zh">此後，</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230534.mp3" url="https://web.klokah.tw/text/sound//17709/230534.mp3"/>
    </S>
    <S id="230535">
        <FORM kindOf="original">waṟal ta' muhuṟow ka Itaṟal la.</FORM>
        <FORM kindOf="standard">waṟal ta' mohoṟow ka Itaṟal la.</FORM>
        <TRANSL xml:lang="zh">泰雅族就繁衍了後代。</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230535.mp3" url="https://web.klokah.tw/text/sound//17709/230535.mp3"/>
    </S>
    <S id="230545">
        <FORM kindOf="original">buṟating! buṟating!</FORM>
        <FORM kindOf="standard">boṟating! boṟating!</FORM>
        <TRANSL xml:lang="zh">月亮!月亮!</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230545.mp3" url="https://web.klokah.tw/text/sound//17711/230545.mp3"/>
    </S>
    <S id="230546">
        <FORM kindOf="original">maṟumul ka buṟating la. marumul cubay!</FORM>
        <FORM kindOf="standard">maṟomol ka boṟating la. maromol cobay!</FORM>
        <TRANSL xml:lang="zh">月圓了，好圓好圓!</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230546.mp3" url="https://web.klokah.tw/text/sound//17711/230546.mp3"/>
    </S>
    <S id="230529">
        <FORM kindOf="original">masaṟugu' cu sipanga' ci ni yaba' mu.</FORM>
        <FORM kindOf="standard">masaṟogo' co sipanga' ci ni yaba' mo.</FORM>
        <TRANSL xml:lang="zh">從爸爸的背上滑下來。</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230529.mp3" url="https://web.klokah.tw/text/sound//17708/230529.mp3"/>
    </S>
    <S id="230531">
        <FORM kindOf="original">pulaka'! pulaka'!</FORM>
        <FORM kindOf="standard">polaka'! polaka'!</FORM>
        <TRANSL xml:lang="zh">飛啊!飛啊!</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230531.mp3" url="https://web.klokah.tw/text/sound//17708/230531.mp3"/>
    </S>
    <S id="230547">
        <FORM kindOf="original">maṟumul ka buṟating cubay.</FORM>
        <FORM kindOf="standard">maṟomol ka boṟating cobay.</FORM>
        <TRANSL xml:lang="zh">好圓好圓的月亮!</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230547.mp3" url="https://web.klokah.tw/text/sound//17711/230547.mp3"/>
    </S>
    <S id="230549">
        <FORM kindOf="original">taxi'! lun'uli' kinang ka buṟating hani'.</FORM>
        <FORM kindOf="standard">taxi'! lon'oli' kinang ka boṟating hani'.</FORM>
        <TRANSL xml:lang="zh">看，月亮一直跟著我!</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230549.mp3" url="https://web.klokah.tw/text/sound//17711/230549.mp3"/>
    </S>
    <S id="230551">
        <FORM kindOf="original">hungu' na amutux! hungu' na amutux!</FORM>
        <FORM kindOf="standard">hongo' na amotox! hongo' na amotox!</FORM>
        <TRANSL xml:lang="zh">神靈之橋!神靈之橋!</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230551.mp3" url="https://web.klokah.tw/text/sound//17712/230551.mp3"/>
    </S>
    <S id="230550">
        <FORM kindOf="original">buṟating, unpabel cu la, pasugagiy ta'la.</FORM>
        <FORM kindOf="standard">boṟating, onpabel co la, pasogagiy ta'la.</FORM>
        <TRANSL xml:lang="zh">月亮，我要睡覺了。晚安!</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230550.mp3" url="https://web.klokah.tw/text/sound//17711/230550.mp3"/>
    </S>
    <S id="230548">
        <FORM kindOf="original">yaba' ka buṟating cubay.</FORM>
        <FORM kindOf="standard">yaba' ka boṟating cobay.</FORM>
        <TRANSL xml:lang="zh">月亮好大唷!</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230548.mp3" url="https://web.klokah.tw/text/sound//17711/230548.mp3"/>
    </S>
    <S id="230552">
        <FORM kindOf="original">pacapung ci linglungan na Itaral ka hungu' na amutux.</FORM>
        <FORM kindOf="standard">pacapong ci linglongan na Itaral ka hongo' na amotox.</FORM>
        <TRANSL xml:lang="zh">由神靈橋裁定泰雅人的良善。</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230552.mp3" url="https://web.klokah.tw/text/sound//17712/230552.mp3"/>
    </S>
    <S id="230553">
        <FORM kindOf="original">ini ta' mamahmut ga,</FORM>
        <FORM kindOf="standard">ini ta' mamahmot ga,</FORM>
        <TRANSL xml:lang="zh">不要胡作非為，</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230553.mp3" url="https://web.klokah.tw/text/sound//17712/230553.mp3"/>
    </S>
    <S id="230554">
        <FORM kindOf="original">pahaṟinas ta' cika' hungu' na amutux.</FORM>
        <FORM kindOf="standard">pahaṟinas ta' cika' hongo' na amotox.</FORM>
        <TRANSL xml:lang="zh">就可以通過神靈之橋。</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230554.mp3" url="https://web.klokah.tw/text/sound//17712/230554.mp3"/>
    </S>
    <S id="230566">
        <FORM kindOf="original">sunkisli'maki' ci ka minakung na baling ka olit xiya'.</FORM>
        <FORM kindOf="standard">sonkisli'maki' ci ka minakong na baling ka olit xiya'.</FORM>
        <TRANSL xml:lang="zh">喜歡住在黑洞裡，</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230566.mp3" url="https://web.klokah.tw/text/sound//17713/230566.mp3"/>
    </S>
    <S id="230567">
        <FORM kindOf="original">malahngan ga matata'tanux moh unkuri' mani' ci nan'iun,</FORM>
        <FORM kindOf="standard">malahngan ga matata'tanox moh onkori' mani' ci nan'ion,</FORM>
        <TRANSL xml:lang="zh">晚上出來偷吃食物，</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230567.mp3" url="https://web.klokah.tw/text/sound//17713/230567.mp3"/>
    </S>
    <S id="230569">
        <FORM kindOf="original">suntatunux caṟung ini bah papabel caminan.</FORM>
        <FORM kindOf="standard">sontatonox caṟong ini bah papabel caminan.</FORM>
        <TRANSL xml:lang="zh">吵得我們睡不著覺。</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230569.mp3" url="https://web.klokah.tw/text/sound//17713/230569.mp3"/>
    </S>
    <S id="230568">
        <FORM kindOf="original">ci! ci! ci! ci! ma' mingaṟu'.</FORM>
        <FORM kindOf="standard">ci! ci! ci! ci! ma' mingaṟo'.</FORM>
        <TRANSL xml:lang="zh">吱吱吱吱的叫，</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230568.mp3" url="https://web.klokah.tw/text/sound//17713/230568.mp3"/>
    </S>
    <S id="230570">
        <FORM kindOf="original">ba' mulata' ka sun malikuṟ na Itaṟal,</FORM>
        <FORM kindOf="standard">ba' molata' ka son malikoṟ na Itaṟal,</FORM>
        <TRANSL xml:lang="zh">泰雅男人會打獵，</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230570.mp3" url="https://web.klokah.tw/text/sound//17714/230570.mp3"/>
    </S>
    <S id="230574">
        <FORM kindOf="original">kelan na turakis cel kagiri',</FORM>
        <FORM kindOf="standard">kelan na torakis cel kagiri',</FORM>
        <TRANSL xml:lang="zh">背簍裡有小米，</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230574.mp3" url="https://web.klokah.tw/text/sound//17715/230574.mp3"/>
    </S>
    <S id="230575">
        <FORM kindOf="original">kelan na singut cel kagiri',</FORM>
        <FORM kindOf="standard">kelan na singot cel kagiri',</FORM>
        <TRANSL xml:lang="zh">背簍裡有樹豆，</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230575.mp3" url="https://web.klokah.tw/text/sound//17715/230575.mp3"/>
    </S>
    <S id="230576">
        <FORM kindOf="original">ramas ni'un ka turakis lu singut.</FORM>
        <FORM kindOf="standard">ramas ni'on ka torakis lo singot.</FORM>
        <TRANSL xml:lang="zh">小米樹豆好好吃。</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230576.mp3" url="https://web.klokah.tw/text/sound//17715/230576.mp3"/>
    </S>
    <S id="230572">
        <FORM kindOf="original">kani ga patasan ka raṟes naha la.</FORM>
        <FORM kindOf="standard">kani ga patasan ka raṟes naha la.</FORM>
        <TRANSL xml:lang="zh">就可以紋面了。</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230572.mp3" url="https://web.klokah.tw/text/sound//17714/230572.mp3"/>
    </S>
    <S id="230573">
        <FORM kindOf="original">mapanga' ci kagiri' ka yaki',</FORM>
        <FORM kindOf="standard">mapanga' ci kagiri' ka yaki',</FORM>
        <TRANSL xml:lang="zh">祖母背著背簍，</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230573.mp3" url="https://web.klokah.tw/text/sound//17715/230573.mp3"/>
    </S>
    <S id="230571">
        <FORM kindOf="original">ba' tuminu' ka sun kanel na Itaṟal,</FORM>
        <FORM kindOf="standard">ba' tomino' ka son kanel na Itaṟal,</FORM>
        <TRANSL xml:lang="zh">泰雅女人會織布，</TRANSL>
        <AUDIO file="圖畫故事篇_Wanda_Atayal_230571.mp3" url="https://web.klokah.tw/text/sound//17714/230571.mp3"/>
    </S>
</TEXT>