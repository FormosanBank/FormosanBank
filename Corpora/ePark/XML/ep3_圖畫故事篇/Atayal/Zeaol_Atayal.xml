<TEXT id="eP3_圖畫故事篇_Zeaol_Atayal" xml:lang="tay" source="ePark3 圖畫故事篇 Zeaol_Atayal" audio="diarized" copyright="CC-BY-NC-SA" citation="Indigenous Languages Research and Development Foundation. (2020). 族語E樂園. https://web.klokah.tw/" BibTeX_citation="@misc{ePark, author = {Indigenous Languages Research and Development Foundation}, title = {族語E樂園}, year = {2020}, url = {https://web.klokah.tw/} }" glottocode="" dialect="Zeaol">
    <S id="229813">
        <FORM kindOf="original">witux! witux! witux!</FORM>
        <FORM kindOf="standard">witox! witox! witox!</FORM>
        <TRANSL xml:lang="zh">太陽!太陽!太陽!</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229813.mp3" url="https://web.klokah.tw/text/sound//17620/229813.mp3"/>
    </S>
    <S id="229814">
        <FORM kindOf="original">smbu na Tayan ku witux ka utux.</FORM>
        <FORM kindOf="standard">smbo na Tayan ko witox ka otox.</FORM>
        <TRANSL xml:lang="zh">泰雅人射下一個太陽。</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229814.mp3" url="https://web.klokah.tw/text/sound//17620/229814.mp3"/>
    </S>
    <S id="229815">
        <FORM kindOf="original">kyan gbyan la.</FORM>
        <FORM kindOf="standard">kyan gbyan la.</FORM>
        <TRANSL xml:lang="zh">從此以後有了晚上。</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229815.mp3" url="https://web.klokah.tw/text/sound//17620/229815.mp3"/>
    </S>
    <S id="229816">
        <FORM kindOf="original">nanu ku haniy?</FORM>
        <FORM kindOf="standard">nano ko haniy?</FORM>
        <TRANSL xml:lang="zh">這是什麼?</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229816.mp3" url="https://web.klokah.tw/text/sound//17621/229816.mp3"/>
    </S>
    <S id="229823">
        <FORM kindOf="original">p'yux ku khawni ru k'man.</FORM>
        <FORM kindOf="standard">p'yox ko khawni ro k'man.</FORM>
        <TRANSL xml:lang="zh">很多樹木和草。</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229823.mp3" url="https://web.klokah.tw/text/sound//17622/229823.mp3"/>
    </S>
    <S id="229822">
        <FORM kindOf="original">bbawi ru mmtasi ku rgyax.</FORM>
        <FORM kindOf="standard">bbawi ro mmtasi ko rgyax.</FORM>
        <TRANSL xml:lang="zh">高聳而深綠的山脈。</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229822.mp3" url="https://web.klokah.tw/text/sound//17622/229822.mp3"/>
    </S>
    <S id="229817">
        <FORM kindOf="original">latan ku haniy.</FORM>
        <FORM kindOf="standard">latan ko haniy.</FORM>
        <TRANSL xml:lang="zh">這是衣服。</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229817.mp3" url="https://web.klokah.tw/text/sound//17621/229817.mp3"/>
    </S>
    <S id="229819">
        <FORM kindOf="original">yupun ku haniy.</FORM>
        <FORM kindOf="standard">yopon ko haniy.</FORM>
        <TRANSL xml:lang="zh">這是褲子。</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229819.mp3" url="https://web.klokah.tw/text/sound//17621/229819.mp3"/>
    </S>
    <S id="229820">
        <FORM kindOf="original">nanu ku haniy?</FORM>
        <FORM kindOf="standard">nano ko haniy?</FORM>
        <TRANSL xml:lang="zh">這是什麼?</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229820.mp3" url="https://web.klokah.tw/text/sound//17621/229820.mp3"/>
    </S>
    <S id="229818">
        <FORM kindOf="original">nanu ku haniy?</FORM>
        <FORM kindOf="standard">nano ko haniy?</FORM>
        <TRANSL xml:lang="zh">這是什麼?</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229818.mp3" url="https://web.klokah.tw/text/sound//17621/229818.mp3"/>
    </S>
    <S id="229825">
        <FORM kindOf="original">kung bbawi tikay! kung bbawi tikay!</FORM>
        <FORM kindOf="standard">kong bbawi tikay! kong bbawi tikay!</FORM>
        <TRANSL xml:lang="zh">再高一點，再高一點!</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229825.mp3" url="https://web.klokah.tw/text/sound//17623/229825.mp3"/>
    </S>
    <S id="229826">
        <FORM kindOf="original">tpanga sku hngali ni yaba.</FORM>
        <FORM kindOf="standard">tpanga sko hngali ni yaba.</FORM>
        <TRANSL xml:lang="zh">騎在爸爸的肩膀上。</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229826.mp3" url="https://web.klokah.tw/text/sound//17623/229826.mp3"/>
    </S>
    <S id="229821">
        <FORM kindOf="original">yamin ru bubing ku haniy.</FORM>
        <FORM kindOf="standard">yamin ro bobing ko haniy.</FORM>
        <TRANSL xml:lang="zh">這是鞋子和帽子。</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229821.mp3" url="https://web.klokah.tw/text/sound//17621/229821.mp3"/>
    </S>
    <S id="229824">
        <FORM kindOf="original">Nguhuw Ruma ku lalu nku alang muw.</FORM>
        <FORM kindOf="standard">Ngohow Roma ko lalo nko alang mow.</FORM>
        <TRANSL xml:lang="zh">他的名字叫努呼路瑪。</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229824.mp3" url="https://web.klokah.tw/text/sound//17622/229824.mp3"/>
    </S>
    <S id="229835">
        <FORM kindOf="original">tmurun ku byating la mtmurun balay!</FORM>
        <FORM kindOf="standard">tmoron ko byating la mtmoron balay!</FORM>
        <TRANSL xml:lang="zh">月圓了，好圓好圓!</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229835.mp3" url="https://web.klokah.tw/text/sound//17625/229835.mp3"/>
    </S>
    <S id="229827">
        <FORM kindOf="original">thogus ru spanga kung ni yaba la.</FORM>
        <FORM kindOf="standard">thogos ro spanga kong ni yaba la.</FORM>
        <TRANSL xml:lang="zh">溜下來讓爸爸背我。</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229827.mp3" url="https://web.klokah.tw/text/sound//17623/229827.mp3"/>
    </S>
    <S id="229829">
        <FORM kindOf="original">plaka! plaka!</FORM>
        <FORM kindOf="standard">plaka! plaka!</FORM>
        <TRANSL xml:lang="zh">飛呀!飛呀!</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229829.mp3" url="https://web.klokah.tw/text/sound//17623/229829.mp3"/>
    </S>
    <S id="229830">
        <FORM kindOf="original">mhtuw sku yamay ku sazing ka tmalung ru utux ka knayrin,</FORM>
        <FORM kindOf="standard">mhtow sko yamay ko sazing ka tmalong ro otox ka knayrin,</FORM>
        <TRANSL xml:lang="zh">兩個男人和一個女人從石頭蹦出來，</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229830.mp3" url="https://web.klokah.tw/text/sound//17624/229830.mp3"/>
    </S>
    <S id="229831">
        <FORM kindOf="original">mhngi sku' gaya nku knayrin ku yangaw,</FORM>
        <FORM kindOf="standard">mhngi sko' gaya nko knayrin ko yangaw,</FORM>
        <TRANSL xml:lang="zh">蒼蠅停留在女人大腿內側，</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229831.mp3" url="https://web.klokah.tw/text/sound//17624/229831.mp3"/>
    </S>
    <S id="229832">
        <FORM kindOf="original">t'aring iy kya la ga,</FORM>
        <FORM kindOf="standard">t'aring iy kya la ga,</FORM>
        <TRANSL xml:lang="zh">此後，</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229832.mp3" url="https://web.klokah.tw/text/sound//17624/229832.mp3"/>
    </S>
    <S id="229833">
        <FORM kindOf="original">memaw iy hbayaw ku Tayan la.</FORM>
        <FORM kindOf="standard">memaw iy hbayaw ko Tayan la.</FORM>
        <TRANSL xml:lang="zh">泰雅族就繁衍了後代。</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229833.mp3" url="https://web.klokah.tw/text/sound//17624/229833.mp3"/>
    </S>
    <S id="229844">
        <FORM kindOf="original">awlit,</FORM>
        <FORM kindOf="standard">awlit,</FORM>
        <TRANSL xml:lang="zh">老鼠，</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229844.mp3" url="https://web.klokah.tw/text/sound//17627/229844.mp3"/>
    </S>
    <S id="229845">
        <FORM kindOf="original">sgalan nku awlit ku mki sku k'yuw ka ini sluy.</FORM>
        <FORM kindOf="standard">sgalan nko awlit ko mki sko k'yow ka ini sloy.</FORM>
        <TRANSL xml:lang="zh">喜歡住在黑洞裡，</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229845.mp3" url="https://web.klokah.tw/text/sound//17627/229845.mp3"/>
    </S>
    <S id="229836">
        <FORM kindOf="original">tmurun ku byating balay.</FORM>
        <FORM kindOf="standard">tmoron ko byating balay.</FORM>
        <TRANSL xml:lang="zh">好圓好圓的月亮。</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229836.mp3" url="https://web.klokah.tw/text/sound//17625/229836.mp3"/>
    </S>
    <S id="229837">
        <FORM kindOf="original">rahu ku byating balay.</FORM>
        <FORM kindOf="standard">raho ko byating balay.</FORM>
        <TRANSL xml:lang="zh">月亮好大唷。</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229837.mp3" url="https://web.klokah.tw/text/sound//17625/229837.mp3"/>
    </S>
    <S id="229838">
        <FORM kindOf="original">txiy! gmluw kuzing ku byating.</FORM>
        <FORM kindOf="standard">txiy! gmlow kozing ko byating.</FORM>
        <TRANSL xml:lang="zh">看!月亮一直跟著我。</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229838.mp3" url="https://web.klokah.tw/text/sound//17625/229838.mp3"/>
    </S>
    <S id="229839">
        <FORM kindOf="original">byating p'abi si la. sgagay ta la.</FORM>
        <FORM kindOf="standard">byating p'abi si la. sgagay ta la.</FORM>
        <TRANSL xml:lang="zh">月亮，我要睡覺了。晚安。</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229839.mp3" url="https://web.klokah.tw/text/sound//17625/229839.mp3"/>
    </S>
    <S id="229851">
        <FORM kindOf="original">blay iy ptasan la.</FORM>
        <FORM kindOf="standard">blay iy ptasan la.</FORM>
        <TRANSL xml:lang="zh">就可以紋面。</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229851.mp3" url="https://web.klokah.tw/text/sound//17628/229851.mp3"/>
    </S>
    <S id="229852">
        <FORM kindOf="original">mpanga na kisi iy yaki.</FORM>
        <FORM kindOf="standard">mpanga na kisi iy yaki.</FORM>
        <TRANSL xml:lang="zh">祖母背著背簍，</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229852.mp3" url="https://web.klokah.tw/text/sound//17629/229852.mp3"/>
    </S>
    <S id="229853">
        <FORM kindOf="original">kyan trakis sku kisi niya.</FORM>
        <FORM kindOf="standard">kyan trakis sko kisi niya.</FORM>
        <TRANSL xml:lang="zh">背簍裡有小米，</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229853.mp3" url="https://web.klokah.tw/text/sound//17629/229853.mp3"/>
    </S>
    <S id="229854">
        <FORM kindOf="original">kyan singut sku kisi niya.</FORM>
        <FORM kindOf="standard">kyan singot sko kisi niya.</FORM>
        <TRANSL xml:lang="zh">背簍裡有樹豆，</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229854.mp3" url="https://web.klokah.tw/text/sound//17629/229854.mp3"/>
    </S>
    <S id="229855">
        <FORM kindOf="original">blay iy ni'un ku trakis ru singut.</FORM>
        <FORM kindOf="standard">blay iy ni'on ko trakis ro singot.</FORM>
        <TRANSL xml:lang="zh">小米樹豆好好吃。</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229855.mp3" url="https://web.klokah.tw/text/sound//17629/229855.mp3"/>
    </S>
    <S id="229849">
        <FORM kindOf="original">ba iy mlyap ku tmalung na Tayan.</FORM>
        <FORM kindOf="standard">ba iy mlyap ko tmalong na Tayan.</FORM>
        <TRANSL xml:lang="zh">泰雅男人會打獵，</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229849.mp3" url="https://web.klokah.tw/text/sound//17628/229849.mp3"/>
    </S>
    <S id="229850">
        <FORM kindOf="original">ba iy tminun ku knayrin na Tayan.</FORM>
        <FORM kindOf="standard">ba iy tminon ko knayrin na Tayan.</FORM>
        <TRANSL xml:lang="zh">泰雅女人會織布，</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229850.mp3" url="https://web.klokah.tw/text/sound//17628/229850.mp3"/>
    </S>
    <S id="229841">
        <FORM kindOf="original">pspung su inlungan na Tayan ku hawngu na Lyutux.</FORM>
        <FORM kindOf="standard">pspong so inlongan na Tayan ko hawngo na Lyotox.</FORM>
        <TRANSL xml:lang="zh">由神靈橋裁定泰雅人的良善。</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229841.mp3" url="https://web.klokah.tw/text/sound//17626/229841.mp3"/>
    </S>
    <S id="229843">
        <FORM kindOf="original">phinas ta sku hawngu na Lyutux.</FORM>
        <FORM kindOf="standard">phinas ta sko hawngo na Lyotox.</FORM>
        <TRANSL xml:lang="zh">就可以通過神靈之橋。</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229843.mp3" url="https://web.klokah.tw/text/sound//17626/229843.mp3"/>
    </S>
    <S id="229834">
        <FORM kindOf="original">byating! byating!</FORM>
        <FORM kindOf="standard">byating! byating!</FORM>
        <TRANSL xml:lang="zh">月亮!月亮!</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229834.mp3" url="https://web.klokah.tw/text/sound//17625/229834.mp3"/>
    </S>
    <S id="229828">
        <FORM kindOf="original">gmapih mnnangi iy mhu'in la.</FORM>
        <FORM kindOf="standard">gmapih mnnangi iy mho'in la.</FORM>
        <TRANSL xml:lang="zh">趴下來，嗯...... 死翹翹了。</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229828.mp3" url="https://web.klokah.tw/text/sound//17623/229828.mp3"/>
    </S>
    <S id="229848">
        <FORM kindOf="original">spapak ru ini samiy abi kwara.</FORM>
        <FORM kindOf="standard">spapak ro ini samiy abi kwara.</FORM>
        <TRANSL xml:lang="zh">吵得我們睡不著覺。</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229848.mp3" url="https://web.klokah.tw/text/sound//17627/229848.mp3"/>
    </S>
    <S id="229842">
        <FORM kindOf="original">ini ta mhmut ga,</FORM>
        <FORM kindOf="standard">ini ta mhmot ga,</FORM>
        <TRANSL xml:lang="zh">不要胡作非為，</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229842.mp3" url="https://web.klokah.tw/text/sound//17626/229842.mp3"/>
    </S>
    <S id="229846">
        <FORM kindOf="original">gbyan ga muwah iy mkuri na nni'un,</FORM>
        <FORM kindOf="standard">gbyan ga mowah iy mkori na nni'on,</FORM>
        <TRANSL xml:lang="zh">晚上出來偷吃食物，</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229846.mp3" url="https://web.klokah.tw/text/sound//17627/229846.mp3"/>
    </S>
    <S id="229847">
        <FORM kindOf="original">ci! ci! ci! ci! maha iy hmuwaw,</FORM>
        <FORM kindOf="standard">ci! ci! ci! ci! maha iy hmowaw,</FORM>
        <TRANSL xml:lang="zh">吱吱吱吱地叫，</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229847.mp3" url="https://web.klokah.tw/text/sound//17627/229847.mp3"/>
    </S>
    <S id="229840">
        <FORM kindOf="original">hawngu na Lyutux! hawngu na Lyutux!</FORM>
        <FORM kindOf="standard">hawngo na Lyotox! hawngo na Lyotox!</FORM>
        <TRANSL xml:lang="zh">神靈之橋!神靈之橋!</TRANSL>
        <AUDIO file="圖畫故事篇_Zeaol_Atayal_229840.mp3" url="https://web.klokah.tw/text/sound//17626/229840.mp3"/>
    </S>
</TEXT>