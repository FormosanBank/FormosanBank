<TEXT id="Siraya_Matthew159" xml:lang="fos" source="Siraya Bible, Matthew gospel" copyright="public domain" citation="" BibTeX_citation="">
  <S id="S1">
    <FORM>ni-pæ-ih-tallagh-nian ymhou-an?</FORM>
  </S>
  <S id="S2">
    <FORM>ka mannana kow, ka ni-pæpæh-</FORM>
  </S>
  <S id="S3">
    <FORM>koulamough-'nian ymhou-an?</FORM>
  </S>
  <S id="S4">
    <FORM>39 Ni-kytta'nian ka manno</FORM>
  </S>
  <S id="S5">
    <FORM>ymhou-an, ka maälam kow, ka ton</FORM>
  </S>
  <S id="S6">
    <FORM>sangat, ka ni-ou-koua'-i-</FORM>
  </S>
  <S id="S7">
    <FORM>hou-an?</FORM>
  </S>
  <S id="S8">
    <FORM>40 Ka matteæi-v'lli-ah ta Mei-</FORM>
  </S>
  <S id="S9">
    <FORM>sasou ka Sibavau mattæ'i-k'ma-</FORM>
  </S>
  <S id="S10">
    <FORM>hynna neini-æn, Missing koun-</FORM>
  </S>
  <S id="S11">
    <FORM>nau-kamou, pa-pynn'an-oumi pæ-</FORM>
  </S>
  <S id="S12">
    <FORM>mut [ta atta] ki saat ki tæ'i-apara-</FORM>
  </S>
  <S id="S13">
    <FORM>au katta ka ousysing, ni-pâmt'n-</FORM>
  </S>
  <S id="S14">
    <FORM>oumi [ta anna] jau-an.</FORM>
  </S>
  <S id="S15">
    <FORM>41 Anna ta mattæ'i-k'ma-al-</FORM>
  </S>
  <S id="S16">
    <FORM>appa-hyuna ki sou neini-æn ka</FORM>
  </S>
  <S id="S17">
    <FORM>itou-u'i, D'marang-â jau-an einou-</FORM>
  </S>
  <S id="S18">
    <FORM>mi ka ni-pahko-avavaghen mou</FORM>
  </S>
  <S id="S19">
    <FORM>koua tou apoei ka myhkakoua</FORM>
  </S>
  <S id="S20">
    <FORM>myddarinouh, ka ni-tanangen ka</FORM>
  </S>
  <S id="S21">
    <FORM>à tou Lyttou-ah ki Tama-Gnau</FORM>
  </S>
  <S id="S22">
    <FORM>appa tyn.</FORM>
  </S>
  <S id="S23">
    <FORM>42 Kani-kagangei-en-au, ka</FORM>
  </S>
  <S id="S24">
    <FORM>assi-moumi ni-pakan jau-an: Ni-</FORM>
  </S>
  <S id="S25">
    <FORM>yht-o-en-au, ka assi-'noumi ni-pæ-</FORM>
  </S>
  <S id="S26">
    <FORM>yt jau-an,</FORM>
  </S>
  <S id="S29">
    <FORM>43 Ni-tæua-en-au, ka</FORM>
  </S>
  <S id="S30">
    <FORM>assinoumi ni-pa-ih-tallagh jau-an:</FORM>
  </S>
  <S id="S31">
    <FORM>Ni-pannana-en-au, ka assi- noumi</FORM>
  </S>
  <S id="S32">
    <FORM>ni-pæpæhkoulamough jau-an: Ni-</FORM>
  </S>
  <S id="S33">
    <FORM>kaälam-en-au, ka ni-itou-koua-en-</FORM>
  </S>
  <S id="S34">
    <FORM>au tou sangat, ka assi-moumi ni-</FORM>
  </S>
  <S id="S35">
    <FORM>k'ytt'an jau-an.</FORM>
  </S>
</TEXT>