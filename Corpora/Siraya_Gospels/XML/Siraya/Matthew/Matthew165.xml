<TEXT id="Siraya_Matthew165" xml:lang="fos" source="Siraya Bible, Matthew gospel" copyright="public domain" citation="" BibTeX_citation="">
  <S id="S1">
    <FORM>30 Jrou ka taka-kahan nein ta</FORM>
  </S>
  <S id="S2">
    <FORM>taka-loulough-an, ni-t'papanah</FORM>
  </S>
  <S id="S3">
    <FORM>ta neni moukoua tou voukkyn ka</FORM>
  </S>
  <S id="S4">
    <FORM>makka-vavoua ki akou-imagh.</FORM>
  </S>
  <S id="S5">
    <FORM>31 Anna ta ni-k'ma ta ti Jesus</FORM>
  </S>
  <S id="S6">
    <FORM>neini-æn, Paka-ymdau-kamou</FORM>
  </S>
  <S id="S7">
    <FORM>paka-varau ki euvan k'atta: Alei</FORM>
  </S>
  <S id="S8">
    <FORM>ka ni-sasoulat-in, Ep'yd-aumau ta</FORM>
  </S>
  <S id="S9">
    <FORM>Souajam, ka papæiävoak-auh ta</FORM>
  </S>
  <S id="S10">
    <FORM>joukko ki jouagh.</FORM>
  </S>
  <S id="S12">
    <FORM>32. Jrou ka hkaha-al-ato-koh</FORM>
  </S>
  <S id="S13">
    <FORM>hpit, tourourou-ah-koh ymou</FORM>
  </S>
  <S id="S14">
    <FORM>mi-æn mou-koua tou Galileen.</FORM>
  </S>
  <S id="S15">
    <FORM>33 Râ ni-mattæ'i-v'lli ta ti Pe-</FORM>
  </S>
  <S id="S16">
    <FORM>trus k'ma tyni-æn, Meita rou paka</FORM>
  </S>
  <S id="S17">
    <FORM>ymd-auh ta neni paka-varau alei</FORM>
  </S>
  <S id="S18">
    <FORM>ymhou-an, myh-kakoua koh assi</FORM>
  </S>
  <S id="S19">
    <FORM>pakavarau-auh ta ti jæu.</FORM>
  </S>
  <S id="S20">
    <FORM>34 Ni-k'ma ta ti Jesus tyni</FORM>
  </S>
  <S id="S21">
    <FORM>æn, Missing kounnau kow, ka</FORM>
  </S>
  <S id="S22">
    <FORM>tou euvan katta dou assi-appa</FORM>
  </S>
  <S id="S23">
    <FORM>mouni ta taoukka, ka t' touroul-</FORM>
  </S>
  <S id="S24">
    <FORM>ei-mhourmau jau-an.</FORM>
  </S>
  <S id="S26">
    <FORM>35 Ni-k'ma ta ti Petrus tyni.</FORM>
  </S>
  <S id="S27">
    <FORM>an, Meita-rou pis-lalam-appa</FORM>
  </S>
  <S id="S28">
    <FORM>mau-kow mapatei, assi-koh rmau-</FORM>
  </S>
  <S id="S29">
    <FORM>ah ymhou-an. Ka komm'appa</FORM>
  </S>
  <S id="S30">
    <FORM>hynna ki sou ta ymmid ki Pahta</FORM>
  </S>
  <S id="S31">
    <FORM>tæutæuughan.</FORM>
  </S>
  <S id="S32">
    <FORM>36 Anna ta ni-toulam neini-</FORM>
  </S>
  <S id="S33">
    <FORM>an ta ti Jesus mou-koua tou jtou-</FORM>
  </S>
  <S id="S34">
    <FORM>kakoua-an ka pananang-en Geth-</FORM>
  </S>
  <S id="S35">
    <FORM>semane, ka ni-k'ma ki Pahtatæu-</FORM>
  </S>
</TEXT>