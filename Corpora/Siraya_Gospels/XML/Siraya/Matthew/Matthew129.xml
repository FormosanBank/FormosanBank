<TEXT id="Siraya_Matthew129" xml:lang="fos" source="Siraya Bible, Matthew gospel" copyright="public domain" citation="" BibTeX_citation="">
  <S id="S1">
    <FORM>28 Ka pah-mang-noumi pah-</FORM>
  </S>
  <S id="S2">
    <FORM>dimdim? Saat ki voual ka akou-</FORM>
  </S>
  <S id="S3">
    <FORM>alak ki darouha, ka rou ou-koua-</FORM>
  </S>
  <S id="S4">
    <FORM>an tyn ki naunamqu,ni-k'ma, Alak</FORM>
  </S>
  <S id="S5">
    <FORM>ou-koua-au, mei-ryng-ei-â hnyn ki</FORM>
  </S>
  <S id="S6">
    <FORM>wa'i katta tou atto-kakaræuma-</FORM>
  </S>
  <S id="S7">
    <FORM>tag-hen-au.</FORM>
  </S>
  <S id="S8">
    <FORM>29 Rå ni-patta'i-vlli-en tyn</FORM>
  </S>
  <S id="S9">
    <FORM>k'ma, Ynnang-koh : si-äugh ki</FORM>
  </S>
  <S id="S10">
    <FORM>atta dou mavoulas, ni-mou-koua-</FORM>
  </S>
  <S id="S11">
    <FORM>ato-ra.</FORM>
  </S>
  <S id="S12">
    <FORM>30 Irou ka qu-koun tyn ki</FORM>
  </S>
  <S id="S13">
    <FORM>kararouha, ni-pa'myh-kaulaula ki</FORM>
  </S>
  <S id="S14">
    <FORM>sou tyni-æn. Ka rou pattæ'i-v'lli-en</FORM>
  </S>
  <S id="S15">
    <FORM>tyn ni-k'ma, Nda mou-koua-ah</FORM>
  </S>
  <S id="S16">
    <FORM>koh, rå assityn vuttin mou-koua.</FORM>
  </S>
  <S id="S17">
    <FORM>31 Timang ki darouha k'atta</FORM>
  </S>
  <S id="S18">
    <FORM>ta ni-pamut ki ka-moei-in ki Ra-</FORM>
  </S>
  <S id="S19">
    <FORM>ma? Ni-k'ma ta neni tyni-æn, Ti</FORM>
  </S>
  <S id="S20">
    <FORM>Naunamou. Ni-k'ma ta ti Jesus</FORM>
  </S>
  <S id="S21">
    <FORM>neini-æn, Missing koun-nau-ka-</FORM>
  </S>
  <S id="S22">
    <FORM>mou-râ, ka tama-jmug ki jnejna-</FORM>
  </S>
  <S id="S23">
    <FORM>appa ka maveioung mou-ouro-ah</FORM>
  </S>
  <S id="S24">
    <FORM>mou-koua ymoumi-æn tou Pei-</FORM>
  </S>
  <S id="S25">
    <FORM>sasou-an ki Alid.</FORM>
  </S>
  <S id="S26">
    <FORM>32 Ka ni-ierroua ta ti Joannes</FORM>
  </S>
  <S id="S27">
    <FORM>ymouni-æn tou darang ki katik-</FORM>
  </S>
  <S id="S28">
    <FORM>tik-an, rå assi-kamou ni-tna-msing</FORM>
  </S>
  <S id="S29">
    <FORM>tyni-æn: rå ni-tna-msing tyni-æn</FORM>
  </S>
  <S id="S30">
    <FORM>ta Tama-jmugh ki jnejna-appa ka</FORM>
  </S>
  <S id="S31">
    <FORM>maveioung. Imoumi-rà dou ni-</FORM>
  </S>
  <S id="S32">
    <FORM>kytta noumi [ta anna] assi-kamou</FORM>
  </S>
  <S id="S33">
    <FORM>ni-malikog mavoulas tou äugh</FORM>
  </S>
  <S id="S34">
    <FORM>alei ki tra-msing-ei-moumi tyni-</FORM>
  </S>
  <S id="S35">
    <FORM>æn,</FORM>
  </S>
</TEXT>