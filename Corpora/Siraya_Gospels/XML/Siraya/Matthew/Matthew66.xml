<TEXT id="Siraya_Matthew66" xml:lang="fos" source="Siraya Bible, Matthew gospel" copyright="public domain" citation="" BibTeX_citation="">
  <S id="S1">
    <FORM>ki d'arang-oho si-ourouro ym-</FORM>
  </S>
  <S id="S2">
    <FORM>hou-an.</FORM>
  </S>
  <S id="S3">
    <FORM>11 Dilligh koun-nau-kamou-</FORM>
  </S>
  <S id="S4">
    <FORM>râ, Neini-æn ka ni-pei-alak ki jnæ-</FORM>
  </S>
  <S id="S5">
    <FORM>jna, assi ni-æhpit ta timamang ka</FORM>
  </S>
  <S id="S6">
    <FORM>tou vavau ki kidi ti Joannes-an ka</FORM>
  </S>
  <S id="S7">
    <FORM>Tama-æulough: Râ teni ka ma-</FORM>
  </S>
  <S id="S8">
    <FORM>saoun tou rarim ki kidi tou Pei-</FORM>
  </S>
  <S id="S9">
    <FORM>sasou-an ki tounnoun ki vullu-vul-</FORM>
  </S>
  <S id="S10">
    <FORM>lum, masaoun-ato tou vavau ki</FORM>
  </S>
  <S id="S11">
    <FORM>kidi teni-æn.</FORM>
  </S>
  <S id="S12">
    <FORM>12 Ka na kidi ki wæi ti Joan-</FORM>
  </S>
  <S id="S13">
    <FORM>nes ka Tama-'æulough tou kidi-</FORM>
  </S>
  <S id="S14">
    <FORM>appa-hnyn, tounnoun-nen ki kailh-</FORM>
  </S>
  <S id="S15">
    <FORM>gen ta Pei-sasou-an ki tounnounki</FORM>
  </S>
  <S id="S16">
    <FORM>ki vullu-bullum, ka neni ka tama-</FORM>
  </S>
  <S id="S17">
    <FORM>tounnoun mourbo tammia ki an-</FORM>
  </S>
  <S id="S18">
    <FORM>na.</FORM>
  </S>
  <S id="S19">
    <FORM>13 Ka ymmid ki na Tama-</FORM>
  </S>
  <S id="S20">
    <FORM>mattæi-tan, ki Tatouhko-appa, ni-</FORM>
  </S>
  <S id="S21">
    <FORM>mattæi-tan tou kidi ti Joannes-an.</FORM>
  </S>
  <S id="S22">
    <FORM>14 Ka rou mamoei-kamou</FORM>
  </S>
  <S id="S23">
    <FORM>læuung [ki sou k'atta] teni ta ti</FORM>
  </S>
  <S id="S24">
    <FORM>Elias ka pa-irou-'auh.</FORM>
  </S>
  <S id="S25">
    <FORM>15 Ti-mamang ka akou-tan-</FORM>
  </S>
  <S id="S26">
    <FORM>gira ki 'æ-illingi-lingig, hmillingi-</FORM>
  </S>
  <S id="S27">
    <FORM>ga-ato.</FORM>
  </S>
  <S id="S28">
    <FORM>16 Ka pæhkaulaul-'au-mau ti-</FORM>
  </S>
  <S id="S29">
    <FORM>mamang-an ta kavoui-vouyl-en</FORM>
  </S>
  <S id="S30">
    <FORM>k'atta? Mahkaulaul ato ki rarau-</FORM>
  </S>
  <S id="S31">
    <FORM>wei ka miraaroung tou 'ǣiræghan,</FORM>
  </S>
  <S id="S32">
    <FORM>ka tmætæm ki lam nein,</FORM>
  </S>
  <S id="S33">
    <FORM>17 Mattæi-k'ma-hynna ki sou,</FORM>
  </S>
  <S id="S34">
    <FORM>Ni-pepariringid-den-j-en-ka-</FORM>
  </S>
  <S id="S35">
    <FORM>mou, ka assi-kamou ni-rmang-toul-</FORM>
  </S>
</TEXT>