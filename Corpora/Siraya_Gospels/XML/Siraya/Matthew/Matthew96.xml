<TEXT id="Siraya_Matthew96" xml:lang="fos" source="Siraya Bible, Matthew gospel" copyright="public domain" citation="" BibTeX_citation="">
  <S id="S1">
    <FORM>29 Irou ka ni-mæli-daæua</FORM>
  </S>
  <S id="S2">
    <FORM>hynna ta ti Jesus, ni-iroua tou ta-</FORM>
  </S>
  <S id="S3">
    <FORM>mang ki vaoung ki Galilea, ka rou</FORM>
  </S>
  <S id="S4">
    <FORM>ni-maddakkin mou-voukin, mier-</FORM>
  </S>
  <S id="S5">
    <FORM>roung-ato hynna.</FORM>
  </S>
  <S id="S6">
    <FORM>30 Ka mabatoung ta makoi-</FORM>
  </S>
  <S id="S7">
    <FORM>lalaulau ka ni-iroua tyni-æn, ka ni-</FORM>
  </S>
  <S id="S8">
    <FORM>pa-toulam neini-æn ki mapilei, ki</FORM>
  </S>
  <S id="S9">
    <FORM>mavoutta, ki mangoupou, ki ma-</FORM>
  </S>
  <S id="S10">
    <FORM>pykka, ki pani-appa ka mararap</FORM>
  </S>
  <S id="S11">
    <FORM>[ki vouel,] ka ni-maki qua neini-</FORM>
  </S>
  <S id="S12">
    <FORM>an matta-douma tou rahpal ti Je</FORM>
  </S>
  <S id="S13">
    <FORM>sus, ka ni-padarikah-en ta neni.</FORM>
  </S>
  <S id="S14">
    <FORM>31 Koumm'appa hynna, ka</FORM>
  </S>
  <S id="S15">
    <FORM>ni-mouh-kiap ta makoi-lalaulau</FORM>
  </S>
  <S id="S16">
    <FORM>dou kakytt'an nein, ka massousou-</FORM>
  </S>
  <S id="S17">
    <FORM>ta mangoupou, ka madarikagh ta</FORM>
  </S>
  <S id="S18">
    <FORM>mapykka, ka mou-næunang ta ma-</FORM>
  </S>
  <S id="S19">
    <FORM>pilei, ka kmyttâ ta mavoutta, ka</FORM>
  </S>
  <S id="S20">
    <FORM>ni-pou-vavau ta neni pou-keirang-</FORM>
  </S>
  <S id="S21">
    <FORM>en ki kidi ki Alid ki Isræul.</FORM>
  </S>
  <S id="S22">
    <FORM>32 Irou ka ni-tætææm ti Je-</FORM>
  </S>
  <S id="S23">
    <FORM>sus ta Pahtatæutæu-han tyn, ni</FORM>
  </S>
  <S id="S24">
    <FORM>k'ma, Mararyh-koh maharas has</FORM>
  </S>
  <S id="S25">
    <FORM>ki kaharum-en ki makoi-lalaulau,</FORM>
  </S>
  <S id="S26">
    <FORM>alei ka makka touro ki wæ'i itou-</FORM>
  </S>
  <S id="S27">
    <FORM>lam jæu-an, ka aousi ki mamang</FORM>
  </S>
  <S id="S28">
    <FORM>ka kan-nau nein: ka ynnang-koh</FORM>
  </S>
  <S id="S29">
    <FORM>pa-darang neini-æn ka assi-appa</FORM>
  </S>
  <S id="S30">
    <FORM>mavok, ka assi-a dou matub tou</FORM>
  </S>
  <S id="S31">
    <FORM>darang-da.</FORM>
  </S>
  <S id="S33">
    <FORM>33 Ka ni-k'ma ta Pahtatæu-</FORM>
  </S>
  <S id="S34">
    <FORM>tæu-han tyn tyni-æn, Toumang</FORM>
  </S>
  <S id="S35">
    <FORM>ta k'mal&lt;unk&gt;ynna ki dægh ki paoul tou</FORM>
  </S>
</TEXT>