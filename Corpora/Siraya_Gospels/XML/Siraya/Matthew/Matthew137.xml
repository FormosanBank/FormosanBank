<TEXT id="Siraya_Matthew137" xml:lang="fos" source="Siraya Bible, Matthew gospel" copyright="public domain" citation="" BibTeX_citation="">
  <S id="S1">
    <FORM>32 Ti jau ta Alid ti Abraham,</FORM>
  </S>
  <S id="S2">
    <FORM>ta Alid ti Isaak, ta Alid-appa ti</FORM>
  </S>
  <S id="S3">
    <FORM>Jakob? Alid ta assi Alid ki patei,</FORM>
  </S>
  <S id="S4">
    <FORM>râ ki mæuah.</FORM>
  </S>
  <S id="S6">
    <FORM>33 Irou ka millingigh[ki atta]</FORM>
  </S>
  <S id="S7">
    <FORM>ta makoilalaulau ni-mauhki-ap-ato</FORM>
  </S>
  <S id="S8">
    <FORM>ki na pahtatæutæuugh tyn.</FORM>
  </S>
  <S id="S9">
    <FORM>34 Rå tou illing-hen ki sari</FORM>
  </S>
  <S id="S10">
    <FORM>séen ka ni tkid tyn ta moutos ki</FORM>
  </S>
  <S id="S11">
    <FORM>Sadduceen, ni-mæua-sal mæuakla</FORM>
  </S>
  <S id="S12">
    <FORM>ta neni.</FORM>
  </S>
  <S id="S14">
    <FORM>35 Ka saat neini-æn-ka Mah-</FORM>
  </S>
  <S id="S15">
    <FORM>tæutæuugh ki Tatouhko ta ni-rih-</FORM>
  </S>
  <S id="S16">
    <FORM>pung tmaligh tyni-æn, k'ma,</FORM>
  </S>
  <S id="S17">
    <FORM>36 Tama-mahtæutæuugh,ka-</FORM>
  </S>
  <S id="S18">
    <FORM>mang ta sou ka pah-kousasousou</FORM>
  </S>
  <S id="S19">
    <FORM>ki keirang-en tou Tatouhko?</FORM>
  </S>
  <S id="S20">
    <FORM>37 Rå ni-k'ma ta ti Jesus ty-</FORM>
  </S>
  <S id="S21">
    <FORM>ni-an, Mava'ango-ah kow ki Mei-</FORM>
  </S>
  <S id="S22">
    <FORM>rang ka Alid oho tou tyntin-noho</FORM>
  </S>
  <S id="S23">
    <FORM>ka myhta tmouk, tou vatti-oho ka</FORM>
  </S>
  <S id="S24">
    <FORM>myhtatmouk, tou ymd-appa ki</FORM>
  </S>
  <S id="S25">
    <FORM>ou-mhga-en hou pah-dimdim.</FORM>
  </S>
  <S id="S26">
    <FORM>38 Atta ta sou ka pahkousasou-</FORM>
  </S>
  <S id="S27">
    <FORM>sou ka naunamou ki keirang-en.</FORM>
  </S>
  <S id="S28">
    <FORM>39 Rå kararouha ta mahkau-</FORM>
  </S>
  <S id="S29">
    <FORM>laula ki atta: Mavæango-ah kow</FORM>
  </S>
  <S id="S30">
    <FORM>ki ta'u ka rarouma mama mama-</FORM>
  </S>
  <S id="S31">
    <FORM>do ymhou-an.</FORM>
  </S>
  <S id="S32">
    <FORM>40 Tou rourouha ki pahkou-</FORM>
  </S>
  <S id="S33">
    <FORM>sasousou k'atta æul-in ta ymmid</FORM>
  </S>
  <S id="S34">
    <FORM>ki Tatouhko ki na Tama-mattai-</FORM>
  </S>
</TEXT>