<TEXT id="Siraya_Matthew147" xml:lang="fos" source="Siraya Bible, Matthew gospel" copyright="public domain" citation="" BibTeX_citation="">
  <S id="S1">
    <FORM>9 Tou kidi-k'anna paä-toun-</FORM>
  </S>
  <S id="S2">
    <FORM>noun-ah ymoumi-æn tou sasamk-</FORM>
  </S>
  <S id="S3">
    <FORM>an, ka pakapatei-ah ymoumi-een&lt;unk&gt;</FORM>
  </S>
  <S id="S4">
    <FORM>ka 'aal-auh-kamou ki ymmid ki</FORM>
  </S>
  <S id="S5">
    <FORM>ta'utaou alei ki Nanang-au.</FORM>
  </S>
  <S id="S6">
    <FORM>10 Ka tou kidi kanna paka</FORM>
  </S>
  <S id="S7">
    <FORM>varau-auh ta madad-agh[ki vouel,]</FORM>
  </S>
  <S id="S8">
    <FORM>ka ma-pæ-vlivlli-ah mapa -</FORM>
  </S>
  <S id="S9">
    <FORM>mado mapa a tounnou tounnoun,</FORM>
  </S>
  <S id="S10">
    <FORM>kamakka-v'llivlli-ah makka-mado</FORM>
  </S>
  <S id="S11">
    <FORM>makka-vahiehyr neini-æn.</FORM>
  </S>
  <S id="S12">
    <FORM>II Ka 'æhpit-ah ta mabæt-</FORM>
  </S>
  <S id="S13">
    <FORM>oung ka assi dlligh ka Tama-mæt-</FORM>
  </S>
  <S id="S14">
    <FORM>tæ'i-tan, ka pataurahei-auh nein</FORM>
  </S>
  <S id="S15">
    <FORM>ta madadagh ki ta'u.</FORM>
  </S>
  <S id="S16">
    <FORM>12 Ka alei-appa ki pakæuhta-</FORM>
  </S>
  <S id="S17">
    <FORM>pang-auh ta katoukoul-an, ka hi-</FORM>
  </S>
  <S id="S18">
    <FORM>oup-eih ki væango ki madadagh</FORM>
  </S>
  <S id="S19">
    <FORM>ki ta'u.</FORM>
  </S>
  <S id="S20">
    <FORM>13 Ka timamang ta itou-tara-</FORM>
  </S>
  <S id="S21">
    <FORM>eugh tou limou-appa, pakariang</FORM>
  </S>
  <S id="S22">
    <FORM>auh ta komma-hynna.</FORM>
  </S>
  <S id="S23">
    <FORM>14 Ka pattasasou-eih ta Ha</FORM>
  </S>
  <S id="S24">
    <FORM>gnau ka mariang k'anna ki Peisa-</FORM>
  </S>
  <S id="S25">
    <FORM>sou-an tou ymmid ki yttou-kakou-</FORM>
  </S>
  <S id="S26">
    <FORM>quan ki Næi, tou pattæni-hailen</FORM>
  </S>
  <S id="S27">
    <FORM>ki ymmid ki ta'utaou ka Heidang:</FORM>
  </S>
  <S id="S28">
    <FORM>anna ta irou-'ah ta limoulymmou.</FORM>
  </S>
  <S id="S29">
    <FORM>15 Jrou ka k'ytt'ei-moumi ta</FORM>
  </S>
  <S id="S30">
    <FORM>kakariskæus-in ki ni-ou-bulbul-en</FORM>
  </S>
  <S id="S31">
    <FORM>ka ni-pæsousou-en ti Daniel ka na</FORM>
  </S>
  <S id="S32">
    <FORM>Tama-mattæ'i-tan, tou itou-koua-</FORM>
  </S>
  <S id="S33">
    <FORM>an ka ni-pou-nni tou tatamd-den ki</FORM>
  </S>
  <S id="S34">
    <FORM>Alid (nda oum-gheilato ki Tama</FORM>
  </S>
  <S id="S35">
    <FORM>kting.)</FORM>
  </S>
</TEXT>