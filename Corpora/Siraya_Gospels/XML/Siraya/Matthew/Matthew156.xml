<TEXT id="Siraya_Matthew156" xml:lang="fos" source="Siraya Bible, Matthew gospel" copyright="public domain" citation="" BibTeX_citation="">
  <S id="S1">
    <FORM>pasi-bavau-aumau kow ki ma-</FORM>
  </S>
  <S id="S2">
    <FORM>dagh: Mou-rbo-à tou karei-en ki</FORM>
  </S>
  <S id="S3">
    <FORM>Meirang oho.</FORM>
  </S>
  <S id="S4">
    <FORM>22 Ka dou moukoua ta teni,</FORM>
  </S>
  <S id="S5">
    <FORM>ka ni-à-douba ki talenten, ni-koua</FORM>
  </S>
  <S id="S6">
    <FORM>tyn, Meirang, dourouha ki talen-</FORM>
  </S>
  <S id="S7">
    <FORM>ten ta ni-p'ha-en-hou jau-an, k'yt-</FORM>
  </S>
  <S id="S8">
    <FORM>t'ei, rourouha-appa ki pa-nni ki</FORM>
  </S>
  <S id="S9">
    <FORM>talenten ka ni-pou-saoun-koh pou</FORM>
  </S>
  <S id="S10">
    <FORM>than.</FORM>
  </S>
  <S id="S11">
    <FORM>23 Ka ni-kma tyni-æn ta Mei-</FORM>
  </S>
  <S id="S12">
    <FORM>rang tyn, Mamsing kouncia ka</FORM>
  </S>
  <S id="S13">
    <FORM>mariang ka makidkid, Ni-makid-</FORM>
  </S>
  <S id="S14">
    <FORM>kid kow katiting-an, pæsi-bavau</FORM>
  </S>
  <S id="S15">
    <FORM>au-mau kow ki madagh: Mou-</FORM>
  </S>
  <S id="S16">
    <FORM>rbo- tou karæi-en ki Meirang</FORM>
  </S>
  <S id="S17">
    <FORM>oho.</FORM>
  </S>
  <S id="S18">
    <FORM>24 Ka dou mou-koua ta teni</FORM>
  </S>
  <S id="S19">
    <FORM>ka ni-æ-sat ki talent, ni-koua tyn,</FORM>
  </S>
  <S id="S20">
    <FORM>Meirang, k'alang-en-au kow ka</FORM>
  </S>
  <S id="S21">
    <FORM>ka'æuloung kow ka maplægh, ka</FORM>
  </S>
  <S id="S22">
    <FORM>majam kow ki assi'mhou na da</FORM>
  </S>
  <S id="S23">
    <FORM>diri, ka pa-lud kow ki assi'mhou</FORM>
  </S>
  <S id="S24">
    <FORM>na sasasbugh.</FORM>
  </S>
  <S id="S26">
    <FORM>25 Ka alei ki tahkout-au ni-</FORM>
  </S>
  <S id="S27">
    <FORM>d'marang-koh hmaa ki talent oho</FORM>
  </S>
  <S id="S28">
    <FORM>tourbo ki næi: k'ytt'ei, akoumea</FORM>
  </S>
  <S id="S29">
    <FORM>kow-ki â'mhou.</FORM>
  </S>
  <S id="S30">
    <FORM>26 Ràni-mattæ'i-v'lli ta Mei-</FORM>
  </S>
  <S id="S31">
    <FORM>rang tyn, k'ma tyni-æn, Kouncia</FORM>
  </S>
  <S id="S32">
    <FORM>ka mate, ka manhgad, kavanna-</FORM>
  </S>
  <S id="S33">
    <FORM>en-hou, ka majam koh tou assi</FORM>
  </S>
  <S id="S34">
    <FORM>mau nad-adiri, ka palud-koh ki</FORM>
  </S>
  <S id="S35">
    <FORM>assi-mæu-na sasasbugh.</FORM>
  </S>
</TEXT>