<TEXT id="Siraya_Matthew167" xml:lang="fos" source="Siraya Bible, Matthew gospel" copyright="public domain" citation="" BibTeX_citation="">
  <S id="S1">
    <FORM>mali-æuragh iauan ta hæuuh</FORM>
  </S>
  <S id="S2">
    <FORM>katta ka ayht-ten, dou assi-mau</FORM>
  </S>
  <S id="S3">
    <FORM>hou-ouro-en myt, nda paanit-auto</FORM>
  </S>
  <S id="S4">
    <FORM>ta kamoei-en-hou.</FORM>
  </S>
  <S id="S5">
    <FORM>43 Ka dou mou-koua ta teni,</FORM>
  </S>
  <S id="S6">
    <FORM>mila kmabalei neini-æn ka meirip:</FORM>
  </S>
  <S id="S7">
    <FORM>alei ka ouhang marisigh ta matta</FORM>
  </S>
  <S id="S8">
    <FORM>nein.</FORM>
  </S>
  <S id="S9">
    <FORM>44 Ka dou payhkakoua neini-</FORM>
  </S>
  <S id="S10">
    <FORM>an, ni-ou-koun tyn mila, ka ni-</FORM>
  </S>
  <S id="S11">
    <FORM>mako-Alilid tou katatourou, mat-</FORM>
  </S>
  <S id="S12">
    <FORM>tæ'i-la teni ki sou-rà.</FORM>
  </S>
  <S id="S13">
    <FORM>45 Anna ta ni-mou-koua ta</FORM>
  </S>
  <S id="S14">
    <FORM>teni ki Pahtatæutæuughan tyn,</FORM>
  </S>
  <S id="S15">
    <FORM>k'ma neini-æn, masinipp-à smaki-</FORM>
  </S>
  <S id="S16">
    <FORM>likough ki mangahar pæ'i-hababa:</FORM>
  </S>
  <S id="S17">
    <FORM>K'ytt'ei matouk-ato ta kidi ki ta'-</FORM>
  </S>
  <S id="S18">
    <FORM>lei, ka pa'a-tounnoun-auh ta</FORM>
  </S>
  <S id="S19">
    <FORM>Alak ki ka'æuloung tou rima ki</FORM>
  </S>
  <S id="S20">
    <FORM>tama-kavarau.</FORM>
  </S>
  <S id="S21">
    <FORM>46 Matti-mæu- æhpit, ou</FORM>
  </S>
  <S id="S22">
    <FORM>koua-ei-mytta, k'ytt'ei, mou-touk-</FORM>
  </S>
  <S id="S23">
    <FORM>ato ta sim'hamha iau-an.</FORM>
  </S>
  <S id="S24">
    <FORM>47 Jrou ka masousou-appa ta</FORM>
  </S>
  <S id="S25">
    <FORM>teni, k'ytt'ei, Ni-iroua ta ti Judas</FORM>
  </S>
  <S id="S26">
    <FORM>ka saät ki sat kytti-æn 'æb ki rou-</FORM>
  </S>
  <S id="S27">
    <FORM>ha, ka toulam tyni-æn ta kalauan</FORM>
  </S>
  <S id="S28">
    <FORM>ka irang ka mei-koua ki tyrrau ki</FORM>
  </S>
  <S id="S29">
    <FORM>vavouvoukbouk, ka ni-padingien</FORM>
  </S>
  <S id="S30">
    <FORM>ki Tama-p'hik ka Sibavau, ki Ma</FORM>
  </S>
  <S id="S31">
    <FORM>moumou-appa ki ta'u.</FORM>
  </S>
  <S id="S32">
    <FORM>48 Ka teni ka si-vanavana ty-</FORM>
  </S>
  <S id="S33">
    <FORM>ni-æn, ni-pou-kidi neini-æn, k'ma,</FORM>
  </S>
  <S id="S34">
    <FORM>Teni ka bæigh-au-mau, teni ta ti</FORM>
  </S>
  <S id="S35">
    <FORM>anna, tmouroung-å tyni-æn.</FORM>
  </S>
</TEXT>