<TEXT id="Siraya_Matthew166" xml:lang="fos" source="Siraya Bible, Matthew gospel" copyright="public domain" citation="" BibTeX_citation="">
  <S id="S1">
    <FORM>tæuughan, Mierrong- hia tou ki-</FORM>
  </S>
  <S id="S2">
    <FORM>di ki ou-koua-eimau hynna mako-</FORM>
  </S>
  <S id="S3">
    <FORM>Alilid.</FORM>
  </S>
  <S id="S4">
    <FORM>37 Jrou ka patou-para tyni,</FORM>
  </S>
  <S id="S5">
    <FORM>æn ti Petrus-an, ki rarouha-appa</FORM>
  </S>
  <S id="S6">
    <FORM>ka na alalak tipaebedeus, ni-ma-</FORM>
  </S>
  <S id="S7">
    <FORM>naunamou mavoulas malouas-ap-</FORM>
  </S>
  <S id="S8">
    <FORM>pa mahymko ta teni.</FORM>
  </S>
  <S id="S9">
    <FORM>38 Anna ta ni-k'ma ta teni</FORM>
  </S>
  <S id="S10">
    <FORM>neini-æn, Mataraæugh mavoulas</FORM>
  </S>
  <S id="S11">
    <FORM>ta vatti-au tou kidi ki kapatei-an&lt;unk&gt;</FORM>
  </S>
  <S id="S12">
    <FORM>jtou-koua hia mahamæuæ'-æ itou-</FORM>
  </S>
  <S id="S13">
    <FORM>lam jau-an.</FORM>
  </S>
  <S id="S14">
    <FORM>39 Jrou ka ni-oukakoukoua</FORM>
  </S>
  <S id="S15">
    <FORM>tyn, ni-mahgakup ki vlung tyn,</FORM>
  </S>
  <S id="S16">
    <FORM>ka ni-mako-Alilid, k'ma, Rama</FORM>
  </S>
  <S id="S17">
    <FORM>au, heirou ka ilpoug-hin, pahali-</FORM>
  </S>
  <S id="S18">
    <FORM>auragha jau-an ki hæuugh k'atta</FORM>
  </S>
  <S id="S19">
    <FORM>ka äyht-tan: Nda assi mama ki ka-</FORM>
  </S>
  <S id="S20">
    <FORM>moei-en-au, râ mam'æppa ki ka-</FORM>
  </S>
  <S id="S21">
    <FORM>moei-en-hou.</FORM>
  </S>
  <S id="S22">
    <FORM>40 Ka ni-mou-kona ta teni ki</FORM>
  </S>
  <S id="S23">
    <FORM>Pahtatautæuughan, ka ni-kmaba</FORM>
  </S>
  <S id="S24">
    <FORM>lei neini-æn ka meirip, ka ni-k'ma</FORM>
  </S>
  <S id="S25">
    <FORM>ti Petrus-an, Maurou-kamou kaua</FORM>
  </S>
  <S id="S26">
    <FORM>mahamauä toulam, jau-an sasat</FORM>
  </S>
  <S id="S27">
    <FORM>kita lei?</FORM>
  </S>
  <S id="S28">
    <FORM>41 Nda mahanaua-</FORM>
  </S>
  <S id="S29">
    <FORM>Alilid-a, ki assi-moumi ou-koua-</FORM>
  </S>
  <S id="S30">
    <FORM>eih tou rpung-an: Massorol ta</FORM>
  </S>
  <S id="S31">
    <FORM>Joepan, rå assi masaukit ta vaat.</FORM>
  </S>
  <S id="S33">
    <FORM>42 Mila tou kararouha dou</FORM>
  </S>
  <S id="S34">
    <FORM>ou-koun tyn, ni-æma-ou-</FORM>
  </S>
  <S id="S35">
    <FORM>Rama-au, heirou assi mali-phough</FORM>
  </S>
</TEXT>