<TEXT id="Siraya_Matthew169" xml:lang="fos" source="Siraya Bible, Matthew gospel" copyright="public domain" citation="" BibTeX_citation="">
  <S id="S1">
    <FORM>t'papænæh-kamou mama ki mou-</FORM>
  </S>
  <S id="S2">
    <FORM>kouah ki Tama-lpogh, ki tyrrau</FORM>
  </S>
  <S id="S3">
    <FORM>ki vavouvouk-bouk, alei ki tou</FORM>
  </S>
  <S id="S4">
    <FORM>roung-au-monmi jauian : wa'i ni-</FORM>
  </S>
  <S id="S5">
    <FORM>mi-sal-koh mierrong ymoumi-æn</FORM>
  </S>
  <S id="S6">
    <FORM>mahtatæutæuugh tou Kouva ki</FORM>
  </S>
  <S id="S7">
    <FORM>tatamd-den ki Alid, ka assikamou</FORM>
  </S>
  <S id="S8">
    <FORM>ni-tmouroung jau-an.</FORM>
  </S>
  <S id="S9">
    <FORM>56 Râ ni-pamt-en ta-mamang</FORM>
  </S>
  <S id="S10">
    <FORM>k'atta alei ka pavut-aul-ato ta na</FORM>
  </S>
  <S id="S11">
    <FORM>sasoulat ki Tama Mattæ'i-tan-da.</FORM>
  </S>
  <S id="S12">
    <FORM>Tou kidi k'anna ni-pou-ymmid</FORM>
  </S>
  <S id="S13">
    <FORM>ilpout ta Pahtatæutæuug-han tyn,</FORM>
  </S>
  <S id="S14">
    <FORM>pa-itou-koua tyni-æn.</FORM>
  </S>
  <S id="S15">
    <FORM>57 Neni ka ni-tmouroung ti</FORM>
  </S>
  <S id="S16">
    <FORM>Jesus-an, ni-mad [tyni-en] pa-</FORM>
  </S>
  <S id="S17">
    <FORM>ou-koua ti Kaisas-an ka Dadyllo</FORM>
  </S>
  <S id="S18">
    <FORM>ka Si-bavau, ni-mæu'i-sâl-hynna</FORM>
  </S>
  <S id="S19">
    <FORM>ta Makou-sasoulat ki Mamou-</FORM>
  </S>
  <S id="S20">
    <FORM>mou-appa.</FORM>
  </S>
  <S id="S21">
    <FORM>58 Ka ni-itou-dallia itou-</FORM>
  </S>
  <S id="S22">
    <FORM>äugh tyni-æn ta ti Petrus tou kidi-</FORM>
  </S>
  <S id="S23">
    <FORM>appa ki las ki Dadyllo, ka rou</FORM>
  </S>
  <S id="S24">
    <FORM>mou-rbo-ato, ni-mi-lam mierrong</FORM>
  </S>
  <S id="S25">
    <FORM>ki Makoua, ka kakytta-auh tyn ta</FORM>
  </S>
  <S id="S26">
    <FORM>lymmou-lymmou.</FORM>
  </S>
  <S id="S27">
    <FORM>59 Râ Tama-P'hik ka Si-bæ-</FORM>
  </S>
