<TEXT id="Siraya_Matthew73" xml:lang="fos" source="Siraya Bible, Matthew gospel" copyright="public domain" citation="" BibTeX_citation="">
  <S id="S1">
    <FORM>26 Ka rou padarang ta Lyttou</FORM>
  </S>
  <S id="S2">
    <FORM>ki Lyttou-an, pa-pah-piah-auh ty-</FORM>
  </S>
  <S id="S3">
    <FORM>ni-æn mamado ka itou kaqua'h</FORM>
  </S>
  <S id="S4">
    <FORM>mama ki mang ta pei-sasou-an tyn?</FORM>
  </S>
  <S id="S5">
    <FORM>27 Irou ka padarang-en-au ta</FORM>
  </S>
  <S id="S6">
    <FORM>Lyttou ki Beelzebul, padarang-en</FORM>
  </S>
  <S id="S7">
    <FORM>ta neni timang-an ki alalak-oumi?</FORM>
  </S>
  <S id="S8">
    <FORM>Alei ki anna Tama-dmung-dung-</FORM>
  </S>
  <S id="S9">
    <FORM>ah ta neni ymoumi-æn</FORM>
  </S>
  <S id="S10">
    <FORM>28 Irou râ padarang-en-au ta</FORM>
  </S>
  <S id="S11">
    <FORM>Lyttou ki Joepan ki Alid, anna</FORM>
  </S>
  <S id="S12">
    <FORM>ta jrou-'ato ymoumi-æn ta Pei-</FORM>
  </S>
  <S id="S13">
    <FORM>sasou-an ki Alid.</FORM>
  </S>
  <S id="S15">
    <FORM>29 Ka mou-lpough-ah mama</FORM>
  </S>
  <S id="S16">
    <FORM>ki mang mourbo ta timamang ki</FORM>
  </S>
  <S id="S17">
    <FORM>tallagh ki siouro maligh pa-sau-</FORM>
  </S>
  <S id="S18">
    <FORM>ara ki satô tyn, rou assi tyn ali-</FORM>
  </S>
  <S id="S19">
    <FORM>ouro-en bæuut ta siouro maligh,</FORM>
  </S>
  <S id="S20">
    <FORM>anna ta pasau-ara-ah ki tallagh</FORM>
  </S>
  <S id="S21">
    <FORM>tyn?</FORM>
  </S>
  <S id="S22">
    <FORM>30 Timamang ta assi tou-lam</FORM>
  </S>
  <S id="S23">
    <FORM>jau-an, teni ta makou-doumma</FORM>
  </S>
  <S id="S24">
    <FORM>jau-an: timamang-appa ta assi pæ-</FORM>
  </S>
  <S id="S25">
    <FORM>pah-kipou pæ-pæ'i-sâl tou-lam</FORM>
  </S>
  <S id="S26">
    <FORM>jau-an, pa-pæh-piæh ta teni.</FORM>
  </S>
  <S id="S27">
    <FORM>31 Alei ki atta pa-sousou-en-</FORM>
  </S>
  <S id="S28">
    <FORM>au-kamou, Ymmid ki varau ki</FORM>
  </S>
  <S id="S29">
    <FORM>barg-han-appa makou-talum atta-</FORM>
  </S>
  <S id="S30">
    <FORM>ral-eih ki kaæuloung: râ barg-han</FORM>
  </S>
  <S id="S31">
    <FORM>makou-talum ka makou-doumma</FORM>
  </S>
  <S id="S32">
    <FORM>ki Joep-an, assi atta-ral-eih ki ka-</FORM>
  </S>
  <S id="S33">
    <FORM>æuloung.</FORM>
  </S>
  <S id="S34">
    <FORM>32 Ka timamang ta masousou-</FORM>
  </S>
  <S id="S35">
    <FORM>al-ato ki mamang ka sou mattæi-</FORM>
  </S>
</TEXT>