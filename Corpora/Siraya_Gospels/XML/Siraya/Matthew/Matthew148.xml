<TEXT id="Siraya_Matthew148" xml:lang="fos" source="Siraya Bible, Matthew gospel" copyright="public domain" citation="" BibTeX_citation="">
  <S id="S1">
    <FORM>16 Tou kidi-k'anna na ma-</FORM>
  </S>
  <S id="S2">
    <FORM>mang ta tou Judea, da illipt-ah</FORM>
  </S>
  <S id="S3">
    <FORM>pou-bævoukkyu.</FORM>
  </S>
  <S id="S4">
    <FORM>17 Na mamang ta a'ia-apang,</FORM>
  </S>
  <S id="S5">
    <FORM>ka assi mou-rarim ki ara-eih tyn ki</FORM>
  </S>
  <S id="S6">
    <FORM>mamang tou tallaghtyn.</FORM>
  </S>
  <S id="S7">
    <FORM>18 Ka æ'ia-ouma-appa, nda</FORM>
  </S>
  <S id="S8">
    <FORM>assi sa-likough-ah ki ara-eih tyn ki</FORM>
  </S>
  <S id="S9">
    <FORM>koulamough.</FORM>
  </S>
  <S id="S11">
    <FORM>19 Ænnai ymoumi [ka jnæ-</FORM>
  </S>
  <S id="S12">
    <FORM>jna ka mavou ka pa-ouho tou</FORM>
  </S>
  <S id="S13">
    <FORM>wæi k'anna.</FORM>
  </S>
  <S id="S14">
    <FORM>20 Nda mako-Alilid-a ka assi</FORM>
  </S>
  <S id="S15">
    <FORM>pavut-auh ta illipt-ei-moumi rou</FORM>
  </S>
  <S id="S16">
    <FORM>amighan, rou wai lava ka pæi-</FORM>
  </S>
  <S id="S17">
    <FORM>habab'æn.</FORM>
  </S>
  <S id="S18">
    <FORM>21 Kairou'ah dou anna ta sa-</FORM>
  </S>
  <S id="S19">
    <FORM>samkan ki bulbul-en ka malouas</FORM>
  </S>
  <S id="S20">
    <FORM>irang, ka ni-aousi ki komma-hyn</FORM>
  </S>
  <S id="S21">
    <FORM>na siæugh ki kidi ki naunamou ki</FORM>
  </S>
  <S id="S22">
    <FORM>Nai tou kidi-appa hnyn, ka aousi.</FORM>
  </S>
  <S id="S23">
    <FORM>al-appa dou roumang.</FORM>
  </S>
  <S id="S24">
    <FORM>22 Ka rou ynna papahkipo-en</FORM>
  </S>
  <S id="S25">
    <FORM>ta kidi ki wæi, assi paka-riang-</FORM>
  </S>
  <S id="S26">
    <FORM>auh ta kamamang ka vaät: rå alei</FORM>
  </S>
  <S id="S27">
    <FORM>ki ni-peri papahkyppo-eih ta wa'i</FORM>
  </S>
  <S id="S28">
    <FORM>k'anna.</FORM>
  </S>
  <S id="S30">
    <FORM>23 Tou kidi k'anna rou mat-</FORM>
  </S>
  <S id="S31">
    <FORM>tæ'i-k'ma-ah-hynna-lava ta tima-</FORM>
  </S>
  <S id="S32">
    <FORM>mang ki sou ymoumi-æn, Kytt'ei,</FORM>
  </S>
  <S id="S33">
    <FORM>tou hia ta ti thristus, tou hynna</FORM>
  </S>
  <S id="S34">
    <FORM>lava, ynna tnamsing-da.</FORM>
  </S>
  <S id="S35">
    <FORM>24 Ka æhpit-ah ta mabatoung</FORM>
  </S>
</TEXT>