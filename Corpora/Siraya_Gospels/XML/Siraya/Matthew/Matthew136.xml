<TEXT id="Siraya_Matthew136" xml:lang="fos" source="Siraya Bible, Matthew gospel" copyright="public domain" citation="" BibTeX_citation="">
  <S id="S1">
    <FORM>alalak, anna ta pou-tbung-ah ta</FORM>
  </S>
  <S id="S2">
    <FORM>tai-a-para tyn ki ni-keiäen tyn,</FORM>
  </S>
  <S id="S3">
    <FORM>ka pææhpit-ah ki æp ki tæi-a-</FORM>
  </S>
  <S id="S4">
    <FORM>para tyn.</FORM>
  </S>
  <S id="S5">
    <FORM>25 Nda na akou-papyttou ki</FORM>
  </S>
  <S id="S6">
    <FORM>mahtæiappapara ymian-æn, ka</FORM>
  </S>
  <S id="S7">
    <FORM>naunamou ka ni-pou-tbung ta ni-</FORM>
  </S>
  <S id="S8">
    <FORM>map-atei-ato, ka alei ka aousi ki</FORM>
  </S>
  <S id="S9">
    <FORM>alalak, ni-pa-tbli ki ni-keiæ tyn ki</FORM>
  </S>
  <S id="S10">
    <FORM>ta'i-apara tyn.</FORM>
  </S>
  <S id="S11">
    <FORM>26</FORM>
  </S>
  <S id="S12">
    <FORM>Ni-mahkaulaula k'omma-</FORM>
  </S>
  <S id="S13">
    <FORM>hynna ta ti kara-rouha, ta'ti kata</FORM>
  </S>
  <S id="S14">
    <FORM>tourou, mah-kidi-appa ki kapa-</FORM>
  </S>
  <S id="S15">
    <FORM>pyttou.</FORM>
  </S>
  <S id="S16">
    <FORM>27 Tou 'äugh ki ymmid nei-</FORM>
  </S>
  <S id="S17">
    <FORM>ni-æn ni-pis-koua-appa mapatei</FORM>
  </S>
  <S id="S18">
    <FORM>ta jna.</FORM>
  </S>
  <S id="S19">
    <FORM>28 Anna ta tou likough-eih</FORM>
  </S>
  <S id="S20">
    <FORM>ahpit, a timang-ah ta keijen ki pa-</FORM>
  </S>
  <S id="S21">
    <FORM>pyttou k'anna? dou ni-pou-ymmid</FORM>
  </S>
  <S id="S22">
    <FORM>nein pou-tbung tyni-æn.</FORM>
  </S>
  <S id="S23">
    <FORM>29 Râ ni-matte'i-vlli ta ti Je-</FORM>
  </S>
  <S id="S24">
    <FORM>sus, k'ma-hia neini-æn, Taura-</FORM>
  </S>
  <S id="S25">
    <FORM>heï-kamou tou assi kavan'an kina</FORM>
  </S>
  <S id="S26">
    <FORM>sasoulat ki ligh-appa ki Alid..</FORM>
  </S>
  <S id="S27">
    <FORM>30 Nda tou likough-eih achpit</FORM>
  </S>
  <S id="S28">
    <FORM>assi pou-tbung-ah, assi papou-</FORM>
  </S>
  <S id="S29">
    <FORM>tbung-an-eih-appa: râ mam'al-ato</FORM>
  </S>
  <S id="S30">
    <FORM>ki Tamahnau ki Alid tou tounoun</FORM>
  </S>
  <S id="S31">
    <FORM>ki vullum.</FORM>
  </S>
  <S id="S33">
    <FORM>31 Ka atou likough-eih æhpit,</FORM>
  </S>
  <S id="S34">
    <FORM>assi-moumi kaua ni-kting ta ni-</FORM>
  </S>
  <S id="S35">
    <FORM>pasousou-en ki Alid ymoumi-æn,</FORM>
  </S>
  <S id="S36">
    <FORM>ka k'ma,</FORM>
  </S>
</TEXT>