<TEXT id="Siraya_Matthew113" xml:lang="fos" source="Siraya Bible, Matthew gospel" copyright="public domain" citation="" BibTeX_citation="">
  <S id="S1">
    <FORM>34 Ka rou mangale ta Mei-</FORM>
  </S>
  <S id="S2">
    <FORM>rang tyn, ni-pæ'ǣ-tounnoun-en</FORM>
  </S>
  <S id="S3">
    <FORM>tyn tyni-æn ki tama-khaut, tou</FORM>
  </S>
  <S id="S4">
    <FORM>kidi-al-appa k'anna ka pou-daauh-</FORM>
  </S>
  <S id="S5">
    <FORM>an-ei tyn ta ymmid ki mamang ka</FORM>
  </S>
  <S id="S6">
    <FORM>kæuitting-en tyn tyni-æn.</FORM>
  </S>
  <S id="S7">
    <FORM>35 Hmei-k'ma-al-appa-hynna</FORM>
  </S>
  <S id="S8">
    <FORM>ymoumi-æn ta Rara-man-au ka</FORM>
  </S>
  <S id="S9">
    <FORM>æ'ia tounnoun ki vullum, rou assi</FORM>
  </S>
  <S id="S10">
    <FORM>kamou matta-raryh ta maihbovoel</FORM>
  </S>
  <S id="S11">
    <FORM>atta-ral ki tæ'i-a-para-oumi ki væ-</FORM>
  </S>
  <S id="S12">
    <FORM>rau nein.</FORM>
  </S>
  <S id="S14">
    <FORM>Ka sasat kytti-an 'ab ki matouda</FORM>
  </S>
  <S id="S15">
    <FORM>kilbeh ki soulat.</FORM>
  </S>
  <S id="S17">
    <FORM>1 KA ni-k'mato-hynna, rou ni-</FORM>
  </S>
  <S id="S18">
    <FORM>mattæ'i-kaha mattæ'i-limou-</FORM>
  </S>
  <S id="S19">
    <FORM>limou ki sou k'atta ta ti Jesus, ka</FORM>
  </S>
  <S id="S20">
    <FORM>ni-d'marang thaboul tou Galilæä,</FORM>
  </S>
  <S id="S21">
    <FORM>ka ni-iroua mou-arogh ki Jordaan</FORM>
  </S>
  <S id="S22">
    <FORM>tou pææpt-ten ki Judæa.</FORM>
  </S>
  <S id="S23">
    <FORM>2 Ka ni-mææuugh tyni-æn ta</FORM>
  </S>
  <S id="S24">
    <FORM>mabatoung ki makoi-lalaulau, ka</FORM>
  </S>
  <S id="S25">
    <FORM>ni-pædærikæh tyn neini-æn to</FORM>
  </S>
  <S id="S26">
    <FORM>hynna.</FORM>
  </S>
  <S id="S27">
    <FORM>3 Ka ni-iroua tyni-æn ta Fa-</FORM>
  </S>
  <S id="S28">
    <FORM>riséen ki rih-pung-ah tyni-en,</FORM>
  </S>
  <S id="S29">
    <FORM>k'ma, Hahei-en kaua ta taou pæ-</FORM>
  </S>
  <S id="S30">
    <FORM>darang ki kæi-æn tyn alei ki ma</FORM>
  </S>
  <S id="S31">
    <FORM>mang</FORM>
  </S>
  <S id="S33">
    <FORM>4 Nda ni-mattæ'i-v'lli ta teni</FORM>
  </S>
  <S id="S34">
    <FORM>k'ma neini-æn, Assi-moumi lava</FORM>
  </S>
  <S id="S35">
    <FORM>ni-ktung, ka teni ka ni-men-ryngei</FORM>
  </S>
</TEXT>