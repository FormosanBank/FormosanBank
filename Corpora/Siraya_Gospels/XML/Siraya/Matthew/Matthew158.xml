<TEXT id="Siraya_Matthew158" xml:lang="fos" source="Siraya Bible, Matthew gospel" copyright="public domain" citation="" BibTeX_citation="">
  <S id="S1">
    <FORM>33 Ka pa-itou-oual-eih tyn ta</FORM>
  </S>
  <S id="S2">
    <FORM>joukko, râ pa-itou-'u'i-eih t&lt;unk&gt;n ta</FORM>
  </S>
  <S id="S3">
    <FORM>jou.</FORM>
  </S>
  <S id="S5">
    <FORM>34 Anna ta mattæ'i-komm.al-</FORM>
  </S>
  <S id="S6">
    <FORM>appa-hynna ki sou ta Mei-sasou</FORM>
  </S>
  <S id="S7">
    <FORM>ka Si-bavau neini-æn ka itou-koua</FORM>
  </S>
  <S id="S8">
    <FORM>tou ouwal tyn, Mou-koua hia ta</FORM>
  </S>
  <S id="S9">
    <FORM>ymoumi ka ni-paka-riang-en-ka-</FORM>
  </S>
  <S id="S10">
    <FORM>mou ki sou ki Rama-au, tabli-ato</FORM>
  </S>
  <S id="S11">
    <FORM>ki Pei-sasouan ka ni tanang-in y-</FORM>
  </S>
  <S id="S12">
    <FORM>mouni-æn na kidi ki ni-pæitou-</FORM>
  </S>
  <S id="S13">
    <FORM>samding-en ta raroun ki Næi.</FORM>
  </S>
  <S id="S14">
    <FORM>35 Ka ni-mahgang-ei-koh, ka</FORM>
  </S>
  <S id="S15">
    <FORM>ni-pakan-noumi jauan: Ni-myt-</FORM>
  </S>
  <S id="S16">
    <FORM>okoh, ka ni-pæ-yt-noumi jæu-an:</FORM>
  </S>
  <S id="S17">
    <FORM>Ni-tæ ækoua-en-at-'mau, ka ni-pæ-</FORM>
  </S>
  <S id="S18">
    <FORM>ih-tatallagh-'noumi jæu-an.</FORM>
  </S>
  <S id="S21">
    <FORM>36 Ni-mannana, ka ni-pæ-</FORM>
  </S>
  <S id="S22">
    <FORM>pah-koulamough-kamou jau-an:</FORM>
  </S>
  <S id="S23">
    <FORM>Ni-malam-kob, ka ni-donoy-ka-</FORM>
  </S>
  <S id="S24">
    <FORM>mou-jæuan Ni-æ'ia-koua-en-au</FORM>
  </S>
  <S id="S25">
    <FORM>tou sangat, ka ni-mou-koua-ka-</FORM>
  </S>
  <S id="S26">
    <FORM>mou jæu-an.</FORM>
  </S>
  <S id="S27">
    <FORM>37 Anna ta mattæ'i-v'lli-ah ty-</FORM>
  </S>
  <S id="S28">
    <FORM>ni-æn ta matiktik, ka k'ma-ah,</FORM>
  </S>
  <S id="S29">
    <FORM>Meirang, ni-k'yttæ'ni-an ka man-</FORM>
  </S>
  <S id="S30">
    <FORM>no ymhou-an ka mahgangei kow,</FORM>
  </S>
  <S id="S31">
    <FORM>ka ni-pækan'nian ymhou-an? ka</FORM>
  </S>
  <S id="S32">
    <FORM>meytto kow, ka ni-pæ-yt'nian ym-</FORM>
  </S>
  <S id="S33">
    <FORM>hou-an?</FORM>
  </S>
  <S id="S34">
    <FORM>38 Ka ni-kytta'nian ka manno</FORM>
  </S>
  <S id="S35">
    <FORM>ymhou-an ka ta'ä-koua kow, ka</FORM>
  </S>
</TEXT>