<TEXT id="Siraya_Matthew175" xml:lang="fos" source="Siraya Bible, Matthew gospel" copyright="public domain" citation="" BibTeX_citation="">
  <S id="S1">
    <FORM>kidi k'anna saat ki vænut-en-ato</FORM>
  </S>
  <S id="S2">
    <FORM>ka mæmægh ki varau, ka ni-saki-</FORM>
  </S>
  <S id="S3">
    <FORM>nanang-en ti Barabbæs.</FORM>
  </S>
  <S id="S4">
    <FORM>17 Jrou ka ni-papæui-sal ta</FORM>
  </S>
  <S id="S5">
    <FORM>neni, ni-k'ma ta ti Pilatus neini-</FORM>
  </S>
  <S id="S6">
    <FORM>æn, Timang ta kamoei-'noumi ka</FORM>
  </S>
  <S id="S7">
    <FORM>aumi-æu-mau-kamou, ti Barabbas</FORM>
  </S>
  <S id="S8">
    <FORM>kaua ti Jesus kaua, ka siouwa ti</FORM>
  </S>
  <S id="S9">
    <FORM>mhristus?</FORM>
  </S>
  <S id="S10">
    <FORM>18 Râ kavanan tyn ka ni.</FORM>
  </S>
  <S id="S11">
    <FORM>paa-tounnoun ta neni tyni-æn.</FORM>
  </S>
  <S id="S12">
    <FORM>alei ki ni-'a'as-sen tyni-æn.</FORM>
  </S>
  <S id="S13">
    <FORM>19 Jrou ka mierrong ta teni</FORM>
  </S>
  <S id="S14">
    <FORM>tou Jerrarong-an ki au-pæpæ-</FORM>
  </S>
  <S id="S15">
    <FORM>nah-hen, ni-madingi ta kei'an tyn</FORM>
  </S>
  <S id="S16">
    <FORM>pakavana tyni-æn, ka komma</FORM>
  </S>
  <S id="S17">
    <FORM>koua, Ynna oumamang ki Matik-</FORM>
  </S>
  <S id="S18">
    <FORM>tik k'anna nda madagh ta ni</FORM>
  </S>
  <S id="S19">
    <FORM>tub'bhen-nau hnyn ki euvan k'atta</FORM>
  </S>
  <S id="S20">
    <FORM>tou malling alei tyni-æn.</FORM>
  </S>
  <S id="S21">
    <FORM>20 Râ ni-pa-tæumha ta Si-bavæu</FORM>
  </S>
  <S id="S22">
    <FORM>ka Tama-Phik ki Mamoumou-</FORM>
  </S>
  <S id="S23">
    <FORM>appa ki makoilalaulau, ka tou-</FORM>
  </S>
  <S id="S24">
    <FORM>mini-au nein ta ti Barabbas, papa-</FORM>
  </S>
  <S id="S25">
    <FORM>kapatei ti Jesus-an.</FORM>
  </S>
  <S id="S27">
    <FORM>21 Ka ni-mattæ'i-vlli ta Tæ''-</FORM>
  </S>
  <S id="S28">
    <FORM>sou ki pourough, k'ma neini-æn,</FORM>
  </S>
  <S id="S29">
    <FORM>Timang ki rarouha katta ta ka-</FORM>
  </S>
  <S id="S30">
    <FORM>moei'noumi ka æumi-au-mau-ka-</FORM>
  </S>
  <S id="S31">
    <FORM>mou? Ka ni-k'ma ta neni, Ti Barab-</FORM>
  </S>
  <S id="S32">
    <FORM>bas.</FORM>
  </S>
  <S id="S33">
    <FORM>22 Ni-k'ma ta ti Pilatus neini-</FORM>
  </S>
  <S id="S34">
    <FORM>an, Mang-aul-at'mau ta ti Jesus,</FORM>
  </S>
  <S id="S35">
    <FORM>ka siouwa ti Nhristus? Ni-kmæi-</FORM>
  </S>
</TEXT>