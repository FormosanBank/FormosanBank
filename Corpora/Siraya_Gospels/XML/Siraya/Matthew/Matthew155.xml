<TEXT id="Siraya_Matthew155" xml:lang="fos" source="Siraya Bible, Matthew gospel" copyright="public domain" citation="" BibTeX_citation="">
  <S id="S1">
    <FORM>15 Kani-pää-rymma-en tyn</FORM>
  </S>
  <S id="S2">
    <FORM>ki talenten ki naunamou, ni-pa'ä-</FORM>
  </S>
  <S id="S3">
    <FORM>roua-en tyn ta kararouha, ni-pa</FORM>
  </S>
  <S id="S4">
    <FORM>sat-en tyn ta katatourou, ta meih-</FORM>
  </S>
  <S id="S5">
    <FORM>boua-voual na kidi ki pasiriang-eih</FORM>
  </S>
  <S id="S6">
    <FORM>tyn pasipyppyou, ka ni-madys tha</FORM>
  </S>
  <S id="S7">
    <FORM>boul.</FORM>
  </S>
  <S id="S8">
    <FORM>16 Ka teni ka ni-a-rymma ki</FORM>
  </S>
  <S id="S9">
    <FORM>talenten, ni-d'marang p'a'i-assa</FORM>
  </S>
  <S id="S10">
    <FORM>ki anna, ka ni-pou-than ki ryryma</FORM>
  </S>
  <S id="S11">
    <FORM>ka pani.</FORM>
  </S>
  <S id="S13">
    <FORM>17 Komm'æppa-hynna ta teni</FORM>
  </S>
  <S id="S14">
    <FORM>ka ni-a-douha, ni-pouthan ki ro-</FORM>
  </S>
  <S id="S15">
    <FORM>rouha ka pani.</FORM>
  </S>
  <S id="S16">
    <FORM>18 Ra teni kani-a-sat mara,</FORM>
  </S>
  <S id="S17">
    <FORM>ni-d marang kmari tou næi, ka</FORM>
  </S>
  <S id="S18">
    <FORM>ni-ha tyn ta malyttouk ki Meirang</FORM>
  </S>
  <S id="S19">
    <FORM>tyn.</FORM>
  </S>
  <S id="S20">
    <FORM>19 Ka siäugh ki madallia ki</FORM>
  </S>
  <S id="S21">
    <FORM>kidi, ni-iroua ta Meirang ki koun</FORM>
  </S>
  <S id="S22">
    <FORM>cia kanna, ka ni-kting tyn mei-</FORM>
  </S>
  <S id="S23">
    <FORM>sal neini-æn.</FORM>
  </S>
  <S id="S24">
    <FORM>20 Kateni ka ni-a-rymma ki</FORM>
  </S>
  <S id="S25">
    <FORM>talenten, ni-mou-koua maad ki ry-</FORM>
  </S>
  <S id="S26">
    <FORM>rymma-appa ka pa-nni ki talenten,</FORM>
  </S>
  <S id="S27">
    <FORM>ka ni-koua tyn, Meirang ryrymma</FORM>
  </S>
  <S id="S28">
    <FORM>ki talenten ka ni-pha-en-hou jaus</FORM>
  </S>
  <S id="S29">
    <FORM>an, k'yttei, ryrymma-appa ka pa</FORM>
  </S>
  <S id="S30">
    <FORM>nni ki talenten, ka ni-pou-saoun-</FORM>
  </S>
  <S id="S31">
    <FORM>koh pou-than ki anna.</FORM>
  </S>
  <S id="S32">
    <FORM>21 Ka ni-k'ma tyni-æn ta</FORM>
  </S>
  <S id="S33">
    <FORM>Meirang tyn, Mamsing, kouncia</FORM>
  </S>
  <S id="S34">
    <FORM>ka mariang ka makidkid, ni-ma</FORM>
  </S>
  <S id="S35">
    <FORM>kidkid kow tou katiting-an, ka</FORM>
  </S>
</TEXT>