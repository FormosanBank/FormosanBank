<TEXT id="Siraya_Matthew163" xml:lang="fos" source="Siraya Bible, Matthew gospel" copyright="public domain" citation="" BibTeX_citation="">
  <S id="S1">
    <FORM>nang-au-mian ymhouan pa-kan ki</FORM>
  </S>
  <S id="S2">
    <FORM>joukko ki Hæli-da'æu-en?</FORM>
  </S>
  <S id="S3">
    <FORM>18 Ka ni-k'ma ta teni, Ou-</FORM>
  </S>
  <S id="S4">
    <FORM>koua-au tou æuma k'anna ti anno-</FORM>
  </S>
  <S id="S5">
    <FORM>an, ka bana tyni-æn, Na sou ki</FORM>
  </S>
  <S id="S6">
    <FORM>Mahtatæutæuuh, tou touk ta ki</FORM>
  </S>
  <S id="S7">
    <FORM>di-au, k-koua-ah-koh kman ym-</FORM>
  </S>
  <S id="S8">
    <FORM>hou-an [ki joukko] ki Hali-da-</FORM>
  </S>
  <S id="S9">
    <FORM>au-en lam ki Pahtatæutzuugh</FORM>
  </S>
  <S id="S10">
    <FORM>en-au.</FORM>
  </S>
  <S id="S11">
    <FORM>19 Ka ni-mei-amut ta Pahta-</FORM>
  </S>
  <S id="S12">
    <FORM>tæutæuugh-an, mama ki na si-a.</FORM>
  </S>
  <S id="S13">
    <FORM>tæutæuugh ti Jesus neini-æn, ka</FORM>
  </S>
  <S id="S14">
    <FORM>ni-patæu-tan nein [ta joukko] ki</FORM>
  </S>
  <S id="S15">
    <FORM>Hali-daæu-en.</FORM>
  </S>
  <S id="S16">
    <FORM>20 Jrou ka mad-oung-ato ni-</FORM>
  </S>
  <S id="S17">
    <FORM>k-lam meirrong ki sat kytti-æn</FORM>
  </S>
  <S id="S18">
    <FORM>'æb ki rouha.</FORM>
  </S>
  <S id="S19">
    <FORM>21 Jrou ka kannin nein, anna</FORM>
  </S>
  <S id="S20">
    <FORM>ta ni k'ma ta teni, Missing koun-</FORM>
  </S>
  <S id="S21">
    <FORM>nau-kamou, ka saät ymoumi-æn</FORM>
  </S>
  <S id="S22">
    <FORM>ta si-vanavana-ah jau-an.</FORM>
  </S>
  <S id="S23">
    <FORM>22 Ka rou ou-hang ta neni</FORM>
  </S>
  <S id="S24">
    <FORM>mivavoulas, ni-mattæi-naunamou</FORM>
  </S>
  <S id="S25">
    <FORM>ta meihbouavou-al neini-æn mat-</FORM>
  </S>
  <S id="S26">
    <FORM>tæ'i-kmahynna tyni-æn, Ti jau</FORM>
  </S>
  <S id="S27">
    <FORM>kaua Meirang?</FORM>
  </S>
  <S id="S28">
    <FORM>23 Ka ni-mattæ'i-v'lli ta teni,</FORM>
  </S>
  <S id="S29">
    <FORM>kma, Teni ka smilam jau-an</FORM>
  </S>
  <S id="S30">
    <FORM>dmæuk ki rima tyn tou tangia, teni</FORM>
  </S>
  <S id="S31">
    <FORM>ta si-vanavana-ah jau-an.</FORM>
  </S>
  <S id="S32">
    <FORM>24 Ouma-ato ka d'marang</FORM>
  </S>
  <S id="S33">
    <FORM>ta Alak ki ka'æuloung mama ki</FORM>
  </S>
  <S id="S34">
    <FORM>nisoulat-en-ato tyni-æn, râ ænnaih</FORM>
  </S>
  <S id="S35">
    <FORM>ki ta'u k'anna ka sivanavana-ah ki</FORM>
  </S>
</TEXT>